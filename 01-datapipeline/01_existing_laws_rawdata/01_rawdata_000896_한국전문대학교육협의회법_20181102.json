cc": "[\uc804\ubb38\uac1c\uc815 2010.3.17]"}, {"@\uc870\ubb38\ud0a4": "0005001", "\uc870\ubb38\ubc88\ud638": "5", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\ucd1d\ud68c", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20130522", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c5\uc870(\ucd1d\ud68c)", "\ud56d": [{"\ud56d\ubc88\ud638": "\u2460", "\ud56d\ub0b4\uc6a9": "\u2460 \ud611\uc758\ud68c\uc5d0 \ucd1d\ud68c\ub97c \ub450\uace0, \ucd1d\ud68c\ub294 \ud68c\uc6d0\uc73c\ub85c \uad6c\uc131\ud55c\ub2e4."}, {"\ud56d\ubc88\ud638": "\u2461", "\ud56d\ub0b4\uc6a9": "\u2461 \ucd1d\ud68c\ub294 \ud611\uc758\ud68c\uc758 \ucd5c\uace0\uc758\uacb0\uae30\uad00\uc73c\ub85c \ud55c\ub2e4."}], "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2010.3.17]"}, {"@\uc870\ubb38\ud0a4": "0006001", "\uc870\ubb38\ubc88\ud638": "6", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uc784\uc6d0", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20130522", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c6\uc870(\uc784\uc6d0)", "\ud56d": [{"\ud56d\ubc88\ud638": "\u2460", "\ud56d\ub0b4\uc6a9": "\u2460 \ud611\uc758\ud68c\uc5d0 \uc784\uc6d0\uc73c\ub85c \ud68c\uc7a5 1\uba85, \ubd80\ud68c\uc7a5 10\uba85 \uc774\ub0b4, \uc774\uc0ac 25\uba85 \uc774\uc0c1 30\uba85 \uc774\ud558, \uac10\uc0ac 2\uba85\uc744 \ub454\ub2e4."}, {"\ud56d\ubc88\ud638": "\u2461", "\ud56d\ub0b4\uc6a9": "\u2461 \uc784\uc6d0\uc740 \ucd1d\ud68c\uc5d0\uc11c \uc120\ucd9c\ud558\ub418, \uad50\uc721\ubd80\uc7a5\uad00\uc758 \uc2b9\uc778\uc744 \ubc1b\uc544\uc57c \ud55c\ub2e4. \ub2e4\ub9cc, \ud68c\uc7a5\u318d\ubd80\ud68c\uc7a5\uc744 \uc81c\uc678\ud55c \uc784\uc6d0\uc758 \uc784\uae30 \uc911 \uacb0\uc6d0\uc774 \uc0dd\uae30\uba74 \uc774\uc0ac\ud68c\uc5d0\uc11c \ubcf4\uad90\uc784\uc6d0\uc744 \uc120\ucd9c\ud55c\ub2e4. <\uac1c\uc815 2013.3.23>"}, {"\ud56d\ubc88\ud638": "\u2462", "\ud56d\ub0b4\uc6a9": "\u2462 \uc784\uc6d0\uc758 \uc784\uae30\ub294 2\ub144\uc73c\ub85c \ud55c\ub2e4. \ub2e4\ub9cc, \ubcf4\uad90\uc784\uc6d0\uc758 \uc784\uae30\ub294 \uc804\uc784\uc790 \uc784\uae30\uc758 \ub0a8\uc740 \uae30\uac04\uc73c\ub85c \ud55c\ub2e4."}, {"\ud56d\ubc88\ud638": "\u2463", "\ud56d\ub0b4\uc6a9": "\u2463 \ud68c\uc7a5\uc740 \ud611\uc758\ud68c\ub97c \ub300\ud45c\ud558\uace0 \ud611\uc758\ud68c\uc758 \uc5c5\ubb34\ub97c \ucd1d\uad04\ud558\uba70, \ubd80\ud68c\uc7a5\uc740 \ud68c\uc7a5\uc744 \ubcf4\uc88c\ud558\uace0 \ud68c\uc7a5\uc774 \ubd80\ub4dd\uc774\ud55c \uc0ac\uc720\ub85c \uc9c1\ubb34\ub97c \uc218\ud589\ud560 \uc218 \uc5c6\uc744 \ub54c\uc5d0 \uadf8 \uc9c1\ubb34\ub97c \ub300\ud589\ud55c\ub2e4."}, {"\ud56d\ubc88\ud638": "\u2464", "\ud56d\ub0b4\uc6a9": "\u2464 \uac10\uc0ac\ub294 \ud611\uc758\ud68c\uc758 \ud68c\uacc4 \ubc0f \ud68c\uacc4\uc640 \uad00\ub828\ub41c \uc5c5\ubb34\ub97c \uac10\uc0ac(\u76e3\u67fb)\ud55c\ub2e4."}], "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2010.3.17]"}, {"@\uc870\ubb38\ud0a4": "0007001", "\uc870\ubb38\ubc88\ud638": "7", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uc774\uc0ac\ud68c", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20130522", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c7\uc870(\uc774\uc0ac\ud68c)", "\ud56d": [{"\ud56d\ubc88\ud638": "\u2460", "\ud56d\ub0b4\uc6a9": "\u2460 \ud611\uc758\ud68c\uc5d0 \uc774\uc0ac\ud68c\ub97c \ub454\ub2e4. \uc774\uc0ac\ud68c\ub294 \ud68c\uc7a5\u318d\ubd80\ud68c\uc7a5\uacfc \uc774\uc0ac\ub85c \uad6c\uc131\ud55c\ub2e4."}, {"\ud56d\ubc88\ud638": "\u2461", "\ud56d\ub0b4\uc6a9": "\u2461 \uc774\uc0ac\ud68c\ub294 \uc774 \ubc95 \ubc0f \uc774 \ubc95\uc5d0 \ub530\ub978 \uba85\ub839\uacfc \uc815\uad00\uc73c\ub85c \uc815\ud558\ub294 \ubc14\uc5d0 \ub530\ub77c \ud611\uc758\ud68c\uc758 \uc911\uc694 \uc0ac\ud56d\uc744 \uc2ec\uc758\u318d\uc758\uacb0\ud55c\ub2e4."}, {"\ud56d\ubc88\ud638": "\u2462", "\ud56d\ub0b4\uc6a9": "\u2462 \uac10\uc0ac\ub294 \uc774\uc0ac\ud68c\uc5d0 \ucc38\uc11d\ud558\uc5ec \ubc1c\uc5b8\ud560 \uc218 \uc788\ub2e4."}], "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2010.3.17]"}, {"@\uc870\ubb38\ud0a4": "0008001", "\uc870\ubb38\ubc88\ud638": "8", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uc0ac\ubb34\ucd1d\uc7a5", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20130522", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c8\uc870(\uc0ac\ubb34\ucd1d\uc7a5)", "\ud56d": [{"\ud56d\ubc88\ud638": "\u2460", "\ud56d\ub0b4\uc6a9": "\u2460 \ucd1d\ud68c \ubc0f \uc774\uc0ac\ud68c\uc5d0\uc11c \uc704\uc784\ud55c \uc0ac\ud56d\uacfc \ud611\uc758\ud68c\uc758 \uc5c5\ubb34\ub97c \uc9d1\ud589\ud558\uae30 \uc704\ud558\uc5ec \ud611\uc758\ud68c\uc5d0 \uc0ac\ubb34\ucd1d\uc7a5\uc744 \ub454\ub2e4."}, {"\ud56d\ubc88\ud638": "\u2461", "\ud56d\ub0b4\uc6a9": "\u2461 \uc0ac\ubb34\ucd1d\uc7a5\uc740 \uc774\uc0ac\ud68c\uc5d0\uc11c \uc120\ucd9c\ud558\ub418, \uad50\uc721\ubd80\uc7a5\uad00\uc758 \uc2b9\uc778\uc744 \ubc1b\uc544 \ud68c\uc7a5\uc774 \uc784\uba85\ud55c\ub2e4. \uc0ac\ubb34\ucd1d\uc7a5\uc740 \uc18c\uc18d \uc9c1\uc6d0\uc744 \uac10\ub3c5\ud558\uace0, \ud68c\uc7a5 \ub610\ub294 \uc774\uc0ac\ud68c\uc5d0\uc11c \uc704\uc784\ud55c \ubc94\uc704\uc5d0\uc11c \ud611\uc758\ud68c\uc758 \uc5c5\ubb34\ub97c \ucd1d\uad04\ud55c\ub2e4. <\uac1c\uc815 2013.3.23>"}], "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2010.3.17]"}, {"@\uc870\ubb38\ud0a4": "0009001", "\uc870\ubb38\ubc88\ud638": "9", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uacbd\ube44\ubcf4\uc870 \ub4f1", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20130522", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c9\uc870(\uacbd\ube44\ubcf4\uc870 \ub4f1)", "\ud56d": [{"\ud56d\ubc88\ud638": "\u2460", "\ud56d\ub0b4\uc6a9": "\u2460 \uad6d\uac00\ub294 \uc608\uc0b0\uc758 \ubc94\uc704\uc5d0\uc11c \ud611\uc758\ud68c\uc758 \uc6b4\uc601\uc5d0 \ud544\uc694\ud55c \uacbd\ube44\ub97c \ubcf4\uc870\ud560 \uc218 \uc788\ub2e4."}, {"\ud56d\ubc88\ud638": "\u2461", "\ud56d\ub0b4\uc6a9": "\u2461 \uac1c\uc778\uc774\ub098 \ubc95\uc778 \ub610\ub294 \ub2e8\uccb4\ub294 \ud611\uc758\ud68c\uc758 \uc2dc\uc124 \ubc0f \uc6b4\uc601\uc744 \uc9c0\uc6d0\ud558\uae30 \uc704\ud558\uc5ec \uae08\uc804\uc774\ub098 \uadf8 \ubc16\uc758 \uc7ac\uc0b0\uc744 \uae30\ubd80\ud560 \uc218 \uc788\ub2e4."}], "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2010.3.17]"}, {"@\uc870\ubb38\ud0a4": "0010001", "\uc870\ubb38\ubc88\ud638": "10", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uad6d\uc720\uc7ac\uc0b0\u318d\uacf5\uc720\uc7ac\uc0b0\uc758 \ub300\ubd80", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20130522", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c10\uc870(\uad6d\uc720\uc7ac\uc0b0\u318d\uacf5\uc720\uc7ac\uc0b0\uc758 \ub300\ubd80)", "\ud56d": [{"\ud56d\ubc88\ud638": "\u2460", "\ud56d\ub0b4\uc6a9": "\u2460 \uad6d\uac00 \ub610\ub294 \uc9c0\ubc29\uc790\uce58\ub2e8\uccb4\ub294 \ud611\uc758\ud68c\uc758 \uc721\uc131\uc744 \uc704\ud558\uc5ec \ud544\uc694\ud558\uba74 \uad6d\uc720\uc7ac\uc0b0 \ub610\ub294 \uacf5\uc720\uc7ac\uc0b0\uc744 \u300c\uad6d\uc720\uc7ac\uc0b0\ubc95\u300d \ub610\ub294 \u300c\uacf5\uc720\uc7ac\uc0b0 \ubc0f \ubb3c\ud488 \uad00\ub9ac\ubc95\u300d\uc5d0\ub3c4 \ubd88\uad6c\ud558\uace0 \ud611\uc7