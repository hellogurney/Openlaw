{"root": {"\ubc95\ub839": {"@\ubc95\ub839\ud0a4": "0011002017072614839", "\uae30\ubcf8\uc815\ubcf4": {"\ubc95\ub839ID": "001100", "\uacf5\ud3ec\uc77c\uc790": "20170726", "\uacf5\ud3ec\ubc88\ud638": "14839", "\uc5b8\uc5b4": "\ud55c\uae00", "\ubc95\uc885\uad6c\ubd84": {"@\ubc95\uc885\uad6c\ubd84\ucf54\ub4dc": "A0002", "#text": "\ubc95\ub960"}, "\ubc95\ub839\uba85_\ud55c\uae00": "\uc11c\uc6b8\ud2b9\ubcc4\uc2dc \ud589\uc815\ud2b9\ub840\uc5d0 \uad00\ud55c \ubc95\ub960", "\ubc95\ub839\uba85_\ud55c\uc790": "\uc11c\uc6b8\ud2b9\ubcc4\uc2dc \ud589\uc815\ud2b9\ub840\uc5d0 \uad00\ud55c \ubc95\ub960", "\ubc95\ub839\uba85\uc57d\uce6d": "\uc11c\uc6b8\uc2dc\ubc95", "\uc81c\uba85\ubcc0\uacbd\uc5ec\ubd80": "N", "\ud55c\uae00\ubc95\ub839\uc5ec\ubd80": "N", "\ud3b8\uc7a5\uc808\uad00": "10020100", "\uc18c\uad00\ubd80\ucc98": {"@\uc18c\uad00\ubd80\ucc98\ucf54\ub4dc": "1741000", "#text": "\ud589\uc815\uc548\uc804\ubd80"}, "\uc2dc\ud589\uc77c\uc790": "20170726", "\uc81c\uac1c\uc815\uad6c\ubd84": "\ud0c0\ubc95\uac1c\uc815", "\ubcc4\ud45c\ud3b8\uc9d1\uc5ec\ubd80": "N", "\uacf5\ud3ec\ubc95\ub839\uc5ec\ubd80": "Y"}, "\uc870\ubb38": {"\uc870\ubb38\ub2e8\uc704": [{"@\uc870\ubb38\ud0a4": "0001001", "\uc870\ubb38\ubc88\ud638": "1", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\ubaa9\uc801", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20170726", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c1\uc870(\ubaa9\uc801) \uc774 \ubc95\uc740 \u300c\uc9c0\ubc29\uc790\uce58\ubc95\u300d \uc81c174\uc870\uc5d0 \ub530\ub77c \uc11c\uc6b8\ud2b9\ubcc4\uc2dc\uc758 \uc9c0\uc704\u318d\uc870\uc9c1 \ubc0f \uc6b4\uc601\uc5d0 \uad00\ud55c \ud2b9\ub840\ub97c \uaddc\uc815\ud568\uc744 \ubaa9\uc801\uc73c\ub85c \ud55c\ub2e4.", "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2011.5.30]"}, {"@\uc870\ubb38\ud0a4": "0002001", "\uc870\ubb38\ubc88\ud638": "2", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uc9c0\uc704", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20170726", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c2\uc870(\uc9c0\uc704) \uc11c\uc6b8\ud2b9\ubcc4\uc2dc\ub294 \uc815\ubd80\uc758 \uc9c1\ud560\ub85c \ub450\ub418, \uc774 \ubc95\uc5d0\uc11c \uc815\ud558\ub294 \ubc94\uc704\uc5d0\uc11c \uc218\ub3c4\ub85c\uc11c\uc758 \ud2b9\uc218\ud55c \uc9c0\uc704\ub97c \uac00\uc9c4\ub2e4.", "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2011.5.30]"}, {"@\uc870\ubb38\ud0a4": "0003001", "\uc870\ubb38\ubc88\ud638": "3", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20170726", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c3\uc870 \uc0ad\uc81c <1994.12.20>"}, {"@\uc870\ubb38\ud0a4": "0004001", "\uc870\ubb38\ubc88\ud638": "4", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uc77c\ubc18\ud589\uc815 \uc6b4\uc601\uc0c1\uc758 \ud2b9\ub840", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20170726", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": "Y", "\uc870\ubb38\ub0b4\uc6a9": "\uc81c4\uc870(\uc77c\ubc18\ud589\uc815 \uc6b4\uc601\uc0c1\uc758 \ud2b9\ub840)", "\ud56d": [{"\ud56d\ubc88\ud638": "\u2460", "\ud56d\uc81c\uac1c\uc815\uc720\ud615": "\uac1c\uc815", "\ud56d\uc81c\uac1c\uc815\uc77c\uc790\ubb38\uc790\uc5f4": "2011.5.30, 2013.3.23, 2014.11.19, 2017.7.26", "\ud56d\ub0b4\uc6a9": "\u2460 \ud589\uc815\uc548\uc804\ubd80\uc7a5\uad00\uc774 \u300c\uc9c0\ubc29\uc7ac\uc815\ubc95\u300d \uc81c11\uc870\uc5d0 \ub530\ub77c \uc11c\uc6b8\ud2b9\ubcc4\uc2dc\uc758 \uc9c0\ubc29\ucc44 \ubc1c\ud589\