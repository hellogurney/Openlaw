, "\ud56d\ub0b4\uc6a9": "\u2461 \uc2ec\uc758\ud68c\ub294 \ub2e4\uc74c \uac01 \ud638\uc758 \uc0ac\ud56d\uc744 \uc2ec\uc758\ud55c\ub2e4.", "\ud638": [{"\ud638\ubc88\ud638": "1.", "\ud638\ub0b4\uc6a9": "1. \uc81c9\uc870\uc5d0 \ub530\ub978 \uc778\ubb38\ud559 \ubc0f \uc778\ubb38\uc815\uc2e0\ubb38\ud654 \uc9c4\ud765\uc5d0 \uad00\ud55c \uc911\uc7a5\uae30 \uc815\ucc45 \ubaa9\ud45c\uc640 \ubc29\ud5a5 \ubc0f \uae30\ubcf8\uacc4\ud68d\uc758 \uc218\ub9bd"}, {"\ud638\ubc88\ud638": "2.", "\ud638\ub0b4\uc6a9": "2. \uc81c10\uc870\uc5d0 \ub530\ub978 \uc2dc\ud589\uacc4\ud68d\uc758 \uc218\ub9bd \ubc0f \ucd94\uc9c4\uc2e4\uc801"}, {"\ud638\ubc88\ud638": "3.", "\ud638\ub0b4\uc6a9": "3. \uc81c18\uc870\uc5d0 \ub530\ub978 \uc804\ub2f4\uae30\uad00\uc758 \uc9c0\uc815"}, {"\ud638\ubc88\ud638": "4.", "\ud638\ub0b4\uc6a9": "4. \uc778\ubb38\ud559 \ubc0f \uc778\ubb38\uc815\uc2e0\ubb38\ud654 \uc9c4\ud765\uc744 \uc704\ud55c \uc81c\ub3c4\uac1c\uc120\uc5d0 \uad00\ud55c \uc0ac\ud56d"}, {"\ud638\ubc88\ud638": "5.", "\ud638\ub0b4\uc6a9": "5. \uadf8 \ubc16\uc5d0 \uc2ec\uc758\ud68c\uc758 \uc5c5\ubb34 \ubc0f \uc6b4\uc601\uacfc \uad00\ub828\ub41c \uc0ac\ud56d\uc73c\ub85c\uc11c \uc704\uc6d0\uc7a5\uc774 \ud68c\uc758\uc5d0 \ubd80\uce58\ub294 \uc0ac\ud56d"}]}, {"\ud56d\ubc88\ud638": "\u2462", "\ud56d\ub0b4\uc6a9": "\u2462 \uc81c2\ud56d\uc5d0 \ub530\ub978 \uc2ec\uc758\ud68c\uc758 \uc2ec\uc758\uc5d0 \uad00\ud55c \uc138\ubd80\uc0ac\ud56d\uc740 \ub300\ud1b5\ub839\ub839\uc73c\ub85c \uc815\ud55c\ub2e4."}]}, {"@\uc870\ubb38\ud0a4": "0007001", "\uc870\ubb38\ubc88\ud638": "7", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "(\uc2ec\uc758\ud68c\uc758 \uad6c\uc131 \ubc0f \uc6b4\uc601)", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20160804", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": "Y", "\uc870\ubb38\ub0b4\uc6a9": "\uc81c7\uc870(\uc2ec\uc758\ud68c\uc758 \uad6c\uc131 \ubc0f \uc6b4\uc601)", "\ud56d": [{"\ud56d\ubc88\ud638": "\u2460", "\ud56d\ub0b4\uc6a9": "\u2460 \uc2ec\uc758\ud68c\ub294 \uc704\uc6d0\uc7a5 1\uba85\uc744 \ud3ec\ud568\ud55c 20\uba85 \uc774\ub0b4\uc758 \uc704\uc6d0\uc73c\ub85c \uad6c\uc131\ud55c\ub2e4."}, {"\ud56d\ubc88\ud638": "\u2461", "\ud56d\ub0b4\uc6a9": "\u2461 \uc704\uc6d0\uc7a5\uc740 \uc81c3\ud638\uc5d0 \ud574\ub2f9\ud558\ub294 \uc0ac\ub78c \uc911\uc5d0\uc11c \uad50\uc721\ubd80\uc7a5\uad00\uacfc \ubb38\ud654\uccb4\uc721\uad00\uad11\ubd80\uc7a5\uad00\uc774 \uacf5\ub3d9\uc73c\ub85c \uc9c0\uba85\ud558\ub294 \uc0ac\ub78c\uc774 \ub418\uba70, \uc704\uc6d0\uc740 \ub2e4\uc74c \uac01 \ud638\uc5d0 \ud574\ub2f9\ud558\ub294 \uc0ac\ub78c\uc774 \ub41c\ub2e4.", "\ud638": [{"\ud638\ubc88\ud638": "1.", "\ud638\ub0b4\uc6a9": "1. \uad50\uc721\ubd80\ucc28\uad00, \ubb38\ud654\uccb4\uc721\uad00\uad11\ubd80\ucc28\uad00(\ubb38\ud654\uccb4\uc721\uad00\uad11\ubd80\uc7a5\uad00\uc774 \uc9c0\uba85\ud558\ub294 \ucc28\uad00), \uadf8 \ubc16\uc5d0 \ub300\ud1b5\ub839\ub839\uc73c\ub85c \uc815\ud558\ub294 \uad00\uacc4 \uc911\uc559\ud589\uc815\uae30\uad00\uc758 \ucc28\uad00\uae09 \uacf5\ubb34\uc6d0"}, {"\ud638\ubc88\ud638": "2.", "\ud638\ub0b4\uc6a9": "2. \uc81c18\uc870\uc5d0 \ub530\ub77c \uc9c0\uc815\ub41c \uc804\ub2f4\uae30\uad00\uc758 \uc7a5 \ubc0f \ub300\ud1b5\ub839\ub839\uc73c\ub85c \uc815\ud558\ub294 \uc778\ubb38\ud559 \ubc0f \uc778\ubb38\uc815\uc2e0\ubb38\ud654 \uc9c4\ud765 \uad00\ub828 \uae30\uad00\uc758 \uc7a5"}, {"\ud638\ubc88\ud638": "3.", "\ud638\ub0b4\uc6a9": "3. \uc778\ubb38\ud559 \ubc0f \uc778\ubb38\uc815\uc2e0\ubb38\ud654\uc5d0 \uad00\ud55c \uc804\ubb38\uc9c0\uc2dd\uacfc \uacbd\ud5d8\uc774 \ud48d\ubd80\ud55c \uc0ac\ub78c \uc911\uc5d0\uc11c \uad50\uc721\ubd80\uc7a5\uad00\uacfc \ubb38\ud654\uccb4\uc721\uad00\uad11\ubd80\uc7a5\uad00\uc774 \uc704\ucd09\ud558\ub294 \uc0ac\ub78c"}]}, {"\ud56d\ubc88\ud638": "\u2462", "\ud56d\ub0b4\uc6a9": "\u2462 \uc704\uc6d0\uc7a5\uc740 \uc2ec\uc758\ud68c\ub97c \ub300\ud45c\ud558\uace0 \uc2ec\uc758\ud68c\uc758 \ud68c\uc758\ub97c \uc18c\uc9d1\ud558\uba70 \uadf8 \uc758\uc7a5\uc774 \ub41c\ub2e4."}, {"\ud56d\ubc88\ud638": "\u2463", "\ud56d\ub0b4\uc6a9": "\u2463 \uadf8 \ubc16\uc5d0 \uc2ec\uc758\ud68c\uc758 \uad6c\uc131 \ubc0f \uc6b4\uc601 \ub4f1\uc5d0 \ud544\uc694\ud55c \uc0ac\ud56d\uc740 \ub300\ud1b5\ub839\ub839\uc73c\ub85c \uc815\ud55c\ub2e4."}]}, {"@\uc870\ubb38\ud0a4": "0008001", "\uc870\ubb38\ubc88\ud638": "8", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "(\uc2ec\uc758\ud68c \uc2ec\uc758\uacb0\uacfc\uc758 \ud65c\uc6a9)", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20160804", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": "Y", "\uc870\ubb38\ub0b4\uc6a9": "\uc81c8\uc870(\uc2ec\uc758\ud68c \uc2ec\uc758\uacb0\uacfc\uc758 \ud65c\uc6a9)", "\ud56d": [{"\ud56d\ubc88\ud638": "\u2460", "\ud56d\ub0b4\uc6a9": "\u2460 \uad50\uc721\ubd80\uc7a5\uad00\uacfc \ubb38\ud654\uccb4\uc721\uad00\uad11\ubd80\uc7a5\uad00\uc740 \uc2ec\uc758\ud68c\uc758 \uc2ec\uc758\uacb0\uacfc\ub97c \uad00\uacc4 \uc911\uc559\ud589\uc815\uae30\uad00\uc758 \uc7a5\uacfc \uc9c0\ubc29\uc790\uce58\ub2e8\uccb4\uc758 \uc7a5\uc5d0\uac8c \uc54c\ub824\uc57c \ud55c\ub2e4."}, {"\ud56d\ubc88\ud638": "\u2461", "\ud56d\ub0b4\uc6a9": "\u2461 \uad00\uacc4 \uc911\uc559\ud589\uc815\uae30\uad00\uc758 \uc7a5\uacfc \uc9c0\ubc29\uc790\uce58\ub2e8\uccb4\uc758 \uc7a5\uc740 \uc2ec\uc758\ud68c\uc758 \uc2ec\uc758\uacb0\uacfc\ub97c \ubc18\uc601\ud558\uc5ec \uc778\ubb38\ud559 \ubc0f \uc778\ubb38\uc815\uc2e0\ubb38\ud654 \uc9c4\ud765 \uc2dc\ucc45\uc744 \uc218\ub9bd\u00b7\uc2dc\ud589\ud558\uc5ec\uc57c \ud55c\ub2e4."}]}, {"@\uc870\ubb38\ud0a4": "0009001", "\uc870\ubb38\ubc88\ud638": "9", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "(\uae30\ubcf8\uacc4\ud68d\uc758 \uc218\ub9bd \ub4f1)", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20160804", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": "Y", "\uc870\ubb38\ub0b4\uc6a9": "\uc81c9\uc870(\uae30\ubcf8\uacc4\ud68d\uc758 \uc218\ub9bd \ub4f1)", "\ud56d": [{"\ud56d\ubc88\ud638": "\u2460", "\ud56d\ub0b4\uc6a9": "\u2460 \uad50\uc721\ubd80\uc7a5\uad00\uacfc \ubb38\ud654\uccb4\uc721\uad00\uad11\ubd80\uc7a5\uad00\uc740 \uc778\ubb38\ud559 \ubc0f \uc778\ubb38\uc815\uc2e0\ubb38\ud654 \uc9c4\ud765\uc5d0 \uad00\ud55c \uc911\uc7a5\uae30 \uc815\ucc45 \ubaa9\ud45c\uc640 \ubc29\ud5a5\uc744 \uc124\uc815\ud558\uace0 \uc2ec\uc758\ud68c\uc758 \uc2ec\uc758\ub97c \uac70\uccd0 \ud655\uc815\ud558\uc5ec\uc57c \ud55c\ub2e4."}, {"\ud56d\ubc88\ud638": "\u2461", "\ud56d\ub0b4\uc6a9": "\u2461 \uad50\uc721\ubd80\uc7a5\uad00\uacfc \ubb38\ud654\uccb4\uc721\uad00\uad11\ubd80\uc7a5\uad00\uc740 \uc81c1\ud56d\uc5d0 \ub530\ub978 \uc778\ubb38\ud559 \ubc0f \uc778\ubb38\uc815\uc2e0\ubb38\ud654 \uc9c4\ud765\uc5d0 \uad00\ud55c \uc911\uc7a5\uae30 \uc815\ucc45 \ubaa9\ud45c\uc640 \ubc29\ud5a5\uc744 \ubc18\uc601\ud558\uace0 \uad00\uacc4 \uc911\uc559\ud589\uc815\uae30\uad00\uc758 \uc778\ubb38\ud559\uacfc \uc778\ubb38\uc815\uc2e0\ubb38\ud654 \uc9c4\ud765 \uad00\ub828 \uacc4\ud68d\uacfc \uc2dc\ucc45 \ub4f1\uc744 \uc885\ud569\ud558\uc5ec 5\ub144\ub9c8\ub2e4 \uc778\ubb38\ud559 \ubc0f \uc778\ubb38\uc815\uc2e0\ubb38\ud654 \uc9c4\ud765 \uae30\ubcf8\uacc4\ud68d(\uc774\ud558 \"\uae30\ubcf8\uacc4\ud68d\"\uc774\ub77c \ud55c\ub2e4)\uc744 \uc138\uc6b0\uace0 \uc2ec\uc758\ud68c\uc758 \uc2ec\uc758\ub97c \uac70\uccd0 \ud655\uc815\ud558\uc5ec\uc57c \ud55c\ub2e4. \uc774 \uacbd\uc6b0 \uad50\uc721\ubd80\uc7a5\uad00\uc740 \uc778\ubb38\ud559 \uc9c4\ud765 \uae30\ubcf8\uacc4\ud68d\uc744, \ubb38\ud654\uccb4\uc721\uad00\uad11\ubd80\uc7a5\uad00\uc740 \uad50\uc721\ubd80\uc7a5\uad00\uacfc \ud611\uc758\ud558\uc5ec \uc778\ubb38\uc815\uc2e0\ubb38\ud654 \uc9c4\ud765 \uae30\ubcf8\uacc4\ud68d\uc744 \uc218\ub9bd\ud558\uace0 \ucd94\uc9c4\ud558\uc5ec\uc57c \ud55c\ub2e4."}, {"\ud56d\ubc88\ud638": "\u2462", "\ud56d\ub0b4\uc6a9": "\u2462 \uae30\ubcf8\uac