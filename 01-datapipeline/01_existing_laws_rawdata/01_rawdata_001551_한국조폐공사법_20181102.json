870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2009.12.29]"}, {"@\uc870\ubb38\ud0a4": "0011001", "\uc870\ubb38\ubc88\ud638": "11", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uc5c5\ubb34", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20140121", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c11\uc870(\uc5c5\ubb34)", "\ud56d": [{"\ud56d\ubc88\ud638": "\u2460", "\ud56d\ub0b4\uc6a9": "\u2460 \uacf5\uc0ac\uc758 \uc5c5\ubb34\ub294 \ub2e4\uc74c \uac01 \ud638\uc640 \uac19\ub2e4.", "\ud638": [{"\ud638\ubc88\ud638": "1.", "\ud638\ub0b4\uc6a9": "1. \uc740\ud589\uad8c\u318d\uc8fc\ud654 \ubc0f \uad6d\ucc44\u318d\uacf5\ucc44\uc758 \uc81c\uc870"}, {"\ud638\ubc88\ud638": "2.", "\ud638\ub0b4\uc6a9": "2. \uc815\ubd80\u318d\uc9c0\ubc29\uc790\uce58\ub2e8\uccb4 \ub610\ub294 \ub300\ud1b5\ub839\ub839\uc73c\ub85c \uc815\ud558\ub294 \uae30\uad00\uc774 \ubc1c\ud589\ud558\ub294 \uac01\uc885 \uc720\uac00\uc99d\uad8c\uc758 \uc81c\uc870"}, {"\ud638\ubc88\ud638": "3.", "\ud638\ub0b4\uc6a9": "3. \uc81c1\ud638 \ubc0f \uc81c2\ud638\uc758 \ubb3c\ud488\uc758 \uc81c\uc870\uc5d0 \ub4dc\ub294 \uac01\uc885 \uc6a9\uc9c0\uc758 \uc81c\uc870"}, {"\ud638\ubc88\ud638": "4.", "\ud638\ub0b4\uc6a9": "4. \uc678\uad6d\uc758 \uc815\ubd80 \ub610\ub294 \uacf5\uacf5\ub2e8\uccb4\ub85c\ubd80\ud130 \uc8fc\ubb38\ubc1b\uc740 \uc81c1\ud638\ubd80\ud130 \uc81c3\ud638\uae4c\uc9c0\uc758 \ubb3c\ud488\uc758 \uc81c\uc870 \ubc0f \uc218\ucd9c"}, {"\ud638\ubc88\ud638": "5.", "\ud638\ub0b4\uc6a9": "5. \ub2e4\uc74c \uac01 \ubaa9\uc5d0 \ub530\ub978 \ubb3c\ud488\uc758 \uc81c\uc870\u318d\ud310\ub9e4 \ubc0f \uc218\ucd9c", "\ubaa9": [{"\ubaa9\ubc88\ud638": "\uac00.", "\ubaa9\ub0b4\uc6a9": "\uac00. \ud2b9\uc218\uc6a9\uc9c0\u318d\ud2b9\uc218\uc778\uc1c4\ubb3c\u318d\ud2b9\uc218\uc555\uc778\ubb3c(\u7279\u6b8a\u62bc\u5370\u7269)"}, {"\ubaa9\ubc88\ud638": "\ub098.", "\ubaa9\ub0b4\uc6a9": "\ub098. \uce74\ub4dc\uc81c\ud488(\ubc1c\uae09\u318d\uc6b4\uc601\uc2dc\uc2a4\ud15c\uc744 \ud3ec\ud568\ud55c\ub2e4)"}, {"\ubaa9\ubc88\ud638": "\ub2e4.", "\ubaa9\ub0b4\uc6a9": "\ub2e4. \uc704\uc870\u318d\ubcc0\uc870 \ubc29\uc9c0\uc6a9 \ubcf4\uc548\uc694\uc18c"}, {"\ubaa9\ubc88\ud638": "\ub77c.", "\ubaa9\ub0b4\uc6a9": "\ub77c. \uc81c1\ud638\ubd80\ud130 \uc81c4\ud638\uae4c\uc9c0\uc640 \uac00\ubaa9\ubd80\ud130 \ub2e4\ubaa9\uae4c\uc9c0\uc5d0 \uaddc\uc815\ub41c \ubb3c\ud488\uc758 \uc704\uc870\u318d\ubcc0\uc870 \uac10\ubcc4\uae30 \ubc0f \uc778\uc2dd\uae30\uae30(\u8a8d\u8b58\u5668\u6a5f)"}, {"\ubaa9\ubc88\ud638": "\ub9c8.", "\ubaa9\ub0b4\uc6a9": "\ub9c8. \uae08\uc18d\uacf5\uc608\ud488. \ub2e4\ub9cc, \ubb38\ud654\uc7ac \uc7ac\ud604\ud488 \ub4f1 \uacf5\uacf5\uc131\uc744 \uac00\uc9c4 \uac83\ub9cc \ud574\ub2f9\ud55c\ub2e4."}]}, {"\ud638\ubc88\ud638": "6.", "\ud638\ub0b4\uc6a9": "6. \ud55c\uad6d\uc740\ud589\uc73c\ub85c\ubd80\ud130\uc758 \ub9e4\uc785 \ub610\ub294 \uc704\ud0c1\uc774\ub098 \uadf8 \ubc16\uc758 \uacc4\uc57d\uc5d0 \ub530\ub978 \uae30\ub150\uc8fc\ud654\u318d\uae30\ub150\uc740\ud589\uad8c\uc758 \ud310\ub9e4"}, {"\ud638\ubc88\ud638": "7.", "\ud638\ub0b4\uc6a9": "7. \uc815\ubd80\ub85c\ubd80\ud130 \uc704\ud0c1\ubc1b\uc740 \uc5c5\ubb34"}, {"\ud638\ubc88\ud638": "8.", "\ud638\ub0b4\uc6a9": "8. \uc81c1\ud638\ubd80\ud130 \uc81c7\ud638\uae4c\uc9c0\uc758 \uc5c5\ubb34\uc640 \uad00\ub828\ub41c \ubb3c\ud488\uc758 \uc0dd\uc0b0\uacfc \uae30\uc220\uac1c\ubc1c \ub4f1\uc744 \uc704\ud55c \uad6d\ub0b4\uc678 \uad00\ub828 \uae30\uad00\u318d\uae30\uc5c5 \ubc0f \ub2e8\uccb4\uc640\uc758 \ud611\ub825(\ubb3c\ud488 \uc0dd\uc0b0\uc758 \ub3c4\uae09, \uae30\uc220\uc81c\ud734 \ub4f1\uc744 \ud3ec\ud568\ud55c\ub2e4)"}, {"\ud638\ubc88\ud638": "9.", "\ud638\ub0b4\uc6a9": "9. \uc81c1\ud638\ubd80\ud130 \uc81c8\ud638\uae4c\uc9c0\uc758 \uc5c5\ubb34\uc5d0 \ub538\ub9b0 \uc5c5\ubb34\ub85c\uc11c \uae30\ud68d\uc7ac\uc815\ubd80\uc7a5\uad00\uc758 \uc778\uac00\ub97c \ubc1b\uc740 \uc5c5\ubb34"}]}, {"\ud56d\ubc88\ud638": "\u2461", "\ud56d\ub0b4\uc6a9": "\u2461 \uacf5\uc0ac\uac00 \uc81c1\ud56d\uc81c5\ud638\uc758 \ubb3c\ud488 \uc911 \uac19\uc740 \ud56d \uc81c1\ud638 \ubc0f \uc81c2\ud638\uc5d0 \ub530\ub978 \ubb3c\ud488\uc758 \uc81c\uc870\uc5d0 \uc9c1\uc811 \uc0ac\uc6a9\ub420 \uc218 \uc788\ub294 \ubb3c\ud488\uc744 \uc815\ubd80\u318d\uc9c0\ubc29\uc790\uce58\ub2e8\uccb4 \ub610\ub294 \ub300\ud1b5\ub839\ub839\uc73c\ub85c \uc815\ud558\ub294 \uae30\uad00\uc774 \uc544\ub2cc \uc790\uc5d0\uac8c \ud310\ub9e4\ud558\uac70\ub098 \uc678\uad6d\uc758 \uc815\ubd80 \ub610\ub294 \uacf5\uacf5\ub2e8\uccb4\uac00 \uc544\ub2cc \uc790\uc5d0\uac8c \uc218\ucd9c\ud558\ub824\ub294 \uacbd\uc6b0\uc5d0\ub294 \ubbf8\ub9ac \uae30\ud68d\uc7ac\uc815\ubd80\uc7a5\uad00\uc758 \uc2b9\uc778\uc744 \ubc1b\uc544\uc57c \ud55c\ub2e4."}, {"\ud56d\ubc88\ud638": "\u2462", "\ud56d\ub0b4\uc6a9": "\u2462 \uacf5\uc0ac\ub294 \uc774\uc0ac\ud68c\uc758 \uc758\uacb0\uc744 \uac70\uccd0 \uc81c1\ud56d \uac01 \ud638\uc758 \uc5c5\ubb34 \ub610\ub294 \uc774\uc640 \uc720\uc0ac\ud55c \uc5c5\ubb34\ub97c \uc218\ud589\ud558\ub294 \ubc95\uc778\uc5d0 \ub300\ud558\uc5ec \uadf8 \uc790\ubcf8\uae08\uc758 \uc804\ubd80 \ub610\ub294 \uc77c\ubd80\ub97c \ucd9c\uc790\ud560 \uc218 \uc788\ub2e4."}], "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2009.12.29]"}, {"@\uc870\ubb38\ud0a4": "0012001", "\uc870\ubb38\ubc88\ud638": "12", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20140121", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c12\uc870 \uc0ad\uc81c <1996\u318d12\u318d31>"}, {"@\uc870\ubb38\ud0a4": "0013001", "\uc870\ubb38\ubc88\ud638": "13", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\ud1f4\uc9c1\uc790\uc5d0 \ub300\ud55c \uc9c0\uc6d0", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20140121", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "0000