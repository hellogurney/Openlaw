ub978\ub2e4."}, {"@\uc870\ubb38\ud0a4": "0005001", "\uc870\ubb38\ubc88\ud638": "5", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uae30\uae08\uc758 \uc6a9\ub3c4", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20171128", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c5\uc870(\uae30\uae08\uc758 \uc6a9\ub3c4)", "\ud56d": [{"\ud56d\ubc88\ud638": "\u2460", "\ud56d\ub0b4\uc6a9": "\u2460 \uae30\uae08\uc740 \ub2e4\uc74c \uac01 \ud638\uc758 \uc5b4\ub290 \ud558\ub098\uc5d0 \ud574\ub2f9\ud558\ub294 \uc6a9\ub3c4\ub85c \ub300\uc5ec(\u8cb8\u8207)\ud560 \uc218 \uc788\ub2e4.", "\ud638": [{"\ud638\ubc88\ud638": "1.", "\ud638\ub0b4\uc6a9": "1. \ud638\ud154\uc744 \ube44\ub86f\ud55c \uac01\uc885 \uad00\uad11\uc2dc\uc124\uc758 \uac74\uc124 \ub610\ub294 \uac1c\uc218(\u6539\u4fee)"}, {"\ud638\ubc88\ud638": "2.", "\ud638\ub0b4\uc6a9": "2. \uad00\uad11\uc744 \uc704\ud55c \uad50\ud1b5\uc218\ub2e8\uc758 \ud655\ubcf4 \ub610\ub294 \uac1c\uc218"}, {"\ud638\ubc88\ud638": "3.", "\ud638\ub0b4\uc6a9": "3. \uad00\uad11\uc0ac\uc5c5\uc758 \ubc1c\uc804\uc744 \uc704\ud55c \uae30\ubc18\uc2dc\uc124\uc758 \uac74\uc124 \ub610\ub294 \uac1c\uc218"}, {"\ud638\ubc88\ud638": "4.", "\ud638\ub0b4\uc6a9": "4. \uad00\uad11\uc9c0\u318d\uad00\uad11\ub2e8\uc9c0 \ubc0f \uad00\uad11\ud2b9\uad6c\uc5d0\uc11c\uc758 \uad00\uad11 \ud3b8\uc758\uc2dc\uc124\uc758 \uac74\uc124 \ub610\ub294 \uac1c\uc218"}]}, {"\ud56d\ubc88\ud638": "\u2461", "\ud56d\ub0b4\uc6a9": "\u2461 \ubb38\ud654\uccb4\uc721\uad00\uad11\ubd80\uc7a5\uad00\uc740 \uae30\uae08\uc5d0\uc11c \uad00\uad11\uc815\ucc45\uc5d0 \uad00\ud558\uc5ec \uc870\uc0ac\u318d\uc5f0\uad6c\ud558\ub294 \ubc95\uc778\uc758 \uae30\ubcf8\uc7ac\uc0b0 \ud615\uc131 \ubc0f \uc870\uc0ac\u318d\uc5f0\uad6c\uc0ac\uc5c5, \uadf8 \ubc16\uc758 \uc6b4\uc601\uc5d0 \ud544\uc694\ud55c \uacbd\ube44\ub97c \ubcf4\uc870\ud560 \uc218 \uc788\ub2e4. <\uac1c\uc815 2008.2.29>"}, {"\ud56d\ubc88\ud638": "\u2462", "\ud56d\ub0b4\uc6a9": "\u2462 \uae30\uae08\uc740 \ub2e4\uc74c \uac01 \ud638\uc758 \uc5b4\ub290 \ud558\ub098\uc5d0 \ud574\ub2f9\ud558\ub294 \uc0ac\uc5c5\uc5d0 \ub300\uc5ec\ud558\uac70\ub098 \ubcf4\uc870\ud560 \uc218 \uc788\ub2e4. <\uac1c\uc815 2009.3.5>", "\ud638": [{"\ud638\ubc88\ud638": "1.", "\ud638\ub0b4\uc6a9": "1. \uad6d\uc678 \uc5ec\ud589\uc790\uc758 \uac74\uc804\ud55c \uad00\uad11\uc744 \uc704\ud55c \uad50\uc721 \ubc0f \uad00\uad11\uc815\ubcf4\uc758 \uc81c\uacf5\uc0ac\uc5c5"}, {"\ud638\ubc88\ud638": "2.", "\ud638\ub0b4\uc6a9": "2. \uad6d\ub0b4\uc678 \uad00\uad11\uc548\ub0b4\uccb4\uacc4\uc758 \uac1c\uc120 \ubc0f \uad00\uad11\ud64d\ubcf4\uc0ac\uc5c5"}, {"\ud638\ubc88\ud638": "3.", "\ud638\ub0b4\uc6a9": "3. \uad00\uad11\uc0ac\uc5c5 \uc885\uc0ac\uc790 \ubc0f \uad00\uacc4\uc790\uc5d0 \ub300\ud55c \uad50\uc721\ud6c8\ub828\uc0ac\uc5c5"}, {"\ud638\ubc88\ud638": "4.", "\ud638\ub0b4\uc6a9": "4. \uad6d\ubbfc\uad00\uad11 \uc9c4\ud765\uc0ac\uc5c5 \ubc0f \uc678\ub798\uad00\uad11\uac1d \uc720\uce58 \uc9c0\uc6d0\uc0ac\uc5c5"}, {"\ud638\ubc88\ud638": "5.", "\ud638\ub0b4\uc6a9": "5. \uad00\uad11\uc0c1\ud488 \uac1c\ubc1c \ubc0f \uc9c0\uc6d0\uc0ac\uc5c5"}, {"\ud638\ubc88\ud638": "6.", "\ud638\ub0b4\uc6a9": "6. \uad00\uad11\uc9c0\u318d\uad00\uad11\ub2e8\uc9c0 \ubc0f \uad00\uad11\ud2b9\uad6c\uc5d0\uc11c\uc758 \uacf5\uacf5 \ud3b8\uc775\uc2dc\uc124 \uc124\uce58\uc0ac\uc5c5"}, {"\ud638\ubc88\ud638": "7.", "\ud638\ub0b4\uc6a9": "7. \uad6d\uc81c\ud68c\uc758\uc758 \uc720\uce58 \ubc0f \uac1c\ucd5c\uc0ac\uc5c5"}, {"\ud638\ubc88\ud638": "8.", "\ud638\ub0b4\uc6a9": "8. \uc7a5\uc560\uc778 \ub4f1 \uc18c\uc678\uacc4\uce35\uc5d0 \ub300\ud55c \uad6d\ubbfc\uad00\uad11 \ubcf5\uc9c0\uc0ac\uc5c5"}, {"\ud638\ubc88\ud638": "9.", "\ud638\ub0b4\uc6a9": "9. \uc804\ud1b5\uad00\uad11\uc790\uc6d0 \uac1c\ubc1c \ubc0f \uc9c0\uc6d0\uc0ac\uc5c5"}, {"\ud638\ubc88\ud638": "10.", "\ud638\ub0b4\uc6a9": "10. \uadf8 \ubc16\uc5d0 \uad00\uad11\uc0ac\uc5c5\uc758 \ubc1c\uc804\uc744 \uc704\ud558\uc5ec \ud544\uc694\ud55c \uac83\uc73c\ub85c\uc11c \ub300\ud1b5\ub839\ub839\uc73c\ub85c \uc815\ud558\ub294 \uc0ac\uc5c5"}]}, {"\ud56d\ubc88\ud638": "\u2463", "\ud56d\ub0b4\uc6a9": "\u2463 \uae30\uae08\uc740 \ubbfc\uac04\uc790\ubcf8\uc758 \uc720\uce58\ub97c \uc704\ud558\uc5ec \ud544\uc694\ud55c \uacbd\uc6b0 \ub2e4\uc74c \uac01 \ud638\uc758 \uc5b4\ub290 \ud558\ub098\uc758 \uc0ac\uc5c5\uc774\ub098 \ud22c\uc790\uc870\ud569\uc5d0 \ucd9c\uc790(\u51fa\u8cc7)\ud560 \uc218 \uc788\ub2e4.", "\ud638": [{"\ud638\ubc88\ud638": "1.", "\ud638\ub0b4\uc6a9": "1. \u300c\uad00\uad11\uc9c4\ud765\ubc95\u300d \uc81c2\uc870\uc81c6\ud638 \ubc0f \uc81c7\ud638\uc5d0 \ub530\ub978 \uad00\uad11\uc9c0 \ubc0f \uad00\uad11\ub2e8\uc9c0\uc758 \uc870\uc131\uc0ac\uc5c5"}, {"\ud638\ubc88\ud638": "2.", "\ud638\ub0b4\uc6a9": "2. \u300c\uad6d\uc81c\ud68c\uc758\uc0b0\uc5c5 \uc721\uc131\uc5d0 \uad00\ud55c \ubc95\ub960\u300d \uc81c2\uc870\uc81c3\ud638\uc5d0 \ub530\ub978 \uad6d\uc81c\ud68c\uc758\uc2dc\uc124\uc758 \uac74\ub9bd \ubc0f \ud655\ucda9 \uc0ac\uc5c5"}, {"\ud638\ubc88\ud638": "3.", "\ud638\ub0b4\uc6a9": "3. \uad00\uad11\uc0ac\uc5c5\uc5d0 \ud22c\uc790\ud558\ub294 \uac83\uc744 \ubaa9\uc801\uc73c\ub85c \ud558\ub294 \ud22c\uc790\uc870\ud569"}, {"\ud638\ubc88\ud638": "4.", "\ud638\ub0b4\uc6a9": "4. \uadf8 \ubc16\uc5d0 \uad00\uad11\uc0ac\uc5c5\uc758 \ubc1c\uc804\uc744 \uc704\ud558\uc5ec \ud544\uc694\ud55c \uac83\uc73c\ub85c\uc11c \ub300\ud1b5\ub839\ub839\uc73c\ub85c \uc815\ud558\ub294 \uc0ac\uc5c5"}]}], "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2007.12.21]"}, {"@\uc870\ubb38\ud0a4": "0006001", "\uc870\ubb38\ubc88\ud638": "6", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uae30\uae08\uc6b4\uc6a9\uc704\uc6d0\ud68c\uc758 \uc124\uce58", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20171128", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc87