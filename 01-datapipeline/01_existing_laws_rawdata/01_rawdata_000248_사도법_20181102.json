790": "20151025", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c2\uc870(\uc815\uc758) \uc774 \ubc95\uc5d0\uc11c \"\uc0ac\ub3c4\"\ub780 \ub2e4\uc74c \uac01 \ud638\uc758 \ub3c4\ub85c\uac00 \uc544\ub2cc \uac83\uc73c\ub85c\uc11c \uadf8 \ub3c4\ub85c\uc5d0 \uc5f0\uacb0\ub418\ub294 \uae38\uc744 \ub9d0\ud55c\ub2e4. \ub2e4\ub9cc, \uc81c3\ud638 \ubc0f \uc81c4\ud638\uc758 \ub3c4\ub85c\ub294 \u300c\ub3c4\ub85c\ubc95\u300d \uc81c50\uc870\uc5d0 \ub530\ub77c \uc2dc\ub3c4(\u5e02\u9053) \ub610\ub294 \uad70\ub3c4(\u90e1\u9053) \uc774\uc0c1\uc5d0 \uc801\uc6a9\ub418\ub294 \ub3c4\ub85c \uad6c\uc870\ub97c \uac16\ucd98 \ub3c4\ub85c\uc5d0 \ud55c\uc815\ud55c\ub2e4. <\uac1c\uc815 2014.1.14>", "\ud56d": {"\ud638": [{"\ud638\ubc88\ud638": "1.", "\ud638\ub0b4\uc6a9": "1. \u300c\ub3c4\ub85c\ubc95\u300d \uc81c2\uc870\uc81c1\ud638\uc5d0 \ub530\ub978 \ub3c4\ub85c"}, {"\ud638\ubc88\ud638": "2.", "\ud638\ub0b4\uc6a9": "2. \u300c\ub3c4\ub85c\ubc95\u300d\uc758 \uc900\uc6a9\uc744 \ubc1b\ub294 \ub3c4\ub85c"}, {"\ud638\ubc88\ud638": "3.", "\ud638\ub0b4\uc6a9": "3. \u300c\ub18d\uc5b4\ucd0c\ub3c4\ub85c \uc815\ube44\ubc95\u300d \uc81c2\uc870\uc81c1\ud56d\uc5d0 \ub530\ub978 \ub18d\uc5b4\ucd0c\ub3c4\ub85c"}, {"\ud638\ubc88\ud638": "4.", "\ud638\ub0b4\uc6a9": "4. \u300c\ub18d\uc5b4\ucd0c\uc815\ube44\ubc95\u300d\uc5d0 \ub530\ub77c \uc124\uce58\ub41c \ub3c4\ub85c"}]}, "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2012.12.18]"}, {"@\uc870\ubb38\ud0a4": "0003001", "\uc870\ubb38\ubc88\ud638": "3", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uc801\uc6a9 \uc81c\uc678", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20151025", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c3\uc870(\uc801\uc6a9 \uc81c\uc678) \uc774 \ubc95\uc740 \ub2e4\uc74c \uac01 \ud638\uc758 \ub3c4\ub85c\uc5d0\ub294 \uc801\uc6a9\ud558\uc9c0 \uc544\ub2c8\ud55c\ub2e4.", "\ud56d": {"\ud638": [{"\ud638\ubc88\ud638": "1.", "\ud638\ub0b4\uc6a9": "1. \ub2e4\ub978 \ubc95\ub960\uc5d0 \ub530\ub77c \uc124\uce58\ud558\ub294 \ub3c4\ub85c"}, {"\ud638\ubc88\ud638": "2.", "\ud638\ub0b4\uc6a9": "2. \uacf5\uc6d0, \uad11\uad6c(\u945b\u5340), \uacf5\uc7a5, \uc8fc\ud0dd\ub2e8\uc9c0, \uadf8 \ubc16\uc5d0 \ub3d9\uc77c\ud55c \uc2dc\uc124 \uc548\uc5d0 \uc124\uce58\ud558\ub294 \ub3c4\ub85c"}]}, "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2012.12.18]"}, {"@\uc870\ubb38\ud0a4": "0004001", "\uc870\ubb38\ubc88\ud638": "4", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uac1c\uc124\ud5c8\uac00 \ub4f1", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20151025", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c4\uc870(\uac1c\uc124\ud5c8\uac00 \ub4f1)", "\ud56d": [{"\ud56d\ubc88\ud638": "\u2460", "\ud56d\ub0b4\uc6a9": "\u2460 \uc0ac\ub3c4\ub97c \uac1c\uc124\u318d\uac1c\ucd95(\u6539\u7bc9)\u318d\uc99d\ucd95(\u589e\u7bc9) \ub610\ub294 \ubcc0\uacbd\ud558\ub824\ub294 \uc790\ub294 \ud2b9\ubcc4\uc790\uce58\uc2dc\uc7a5, \ud2b9\ubcc4\uc790\uce58\ub3c4\uc9c0\uc0ac \ub610\ub294 \uc2dc\uc7a5\u318d\uad70\uc218\u318d\uad6c\uccad\uc7a5(\uad6c\uccad\uc7a5\uc740 \uc790\uce58\uad6c\uc758 \uad6c\uccad\uc7a5\uc744 \ub9d0\ud558\uba70, \uc774\ud558 \"\uc2dc\uc7a5\u318d\uad70\uc218\u318d\uad6c\uccad\uc7a5\"\uc774\ub77c \ud55c\ub2e4)\uc758 \ud5c8\uac00\ub97c \ubc1b\uc544\uc57c \ud55c\ub2e4."}, {"\ud56d\ubc88\ud638": "\u2461", "\ud56d\ub0b4\uc6a9": "\u2461 \uc81c1\ud56d\uc5d0 \ub530\ub978 \ud5c8\uac00\ub97c \ubc1b\uc73c\ub824\ub294 \uc790\ub294 \ud5c8\uac00\uc2e0\uccad\uc11c\uc5d0 \uad6d\ud1a0\uad50\ud1b5\ubd80\ub839\uc73c\ub85c \uc815\ud558\ub294 \uc11c\ub958\ub97c \ucca8\ubd80\ud558\uc5ec \uc2dc\uc7a5\u318d\uad70\uc218\u318d\uad6c\uccad\uc7a5\uc5d0\uac8c \uc81c\ucd9c\ud558\uc5ec\uc57c \ud55c\ub2e4. <\uac1c\uc815 2013.3.23>"}, {"\ud56d\ubc88\ud638": "\u2462", "\ud56d\ub0b4\uc6a9": "\u2462 \uc2dc\uc7a5\u318d\uad70\uc218\u318d\uad6c\uccad\uc7a5\uc740 \ub2e4\uc74c \uac01 \ud638\uc758 \uc5b4\ub290 \ud558\ub098\uc5d0 \ud574\ub2f9\ud558\ub294 \uacbd\uc6b0\ub97c \uc81c\uc678\ud558\uace0\ub294 \uc81c1\ud56d\uc5d0 \ub530\ub978 \ud5c8\uac00\ub97c \ud558\uc5ec\uc57c \ud55c\ub2e4.", "\ud638": [{"\ud638\ubc88\ud638": "1.", "\ud638\ub0b4\uc6a9": "1. \uac1c\uc124\ud558\ub824\ub294 \uc0ac\ub3c4\uac00 \uc81c5\uc870\uc5d0 \ub530\ub978 \uae30\uc900\uc5d0 \ub9de\uc9c0 \uc544\ub2c8\ud55c \uacbd\uc6b0"}, {"\ud638\ubc88\ud638": "2.", "\ud638\ub0b4\uc6a9": "2. \ud5c8\uac00\ub97c \uc2e0\uccad\ud55c \uc790\uc5d0\uac8c \ud574\ub2f9 \ud1a0\uc9c0\uc758 \uc18c\uc720 \ub610\ub294 \uc0ac\uc6a9\uc5d0 \uad00\ud55c \uad8c\ub9ac\uac00 \uc5c6\ub294 \uacbd\uc6b0"}, {"\ud638\ubc88\ud638": "3.", "\ud638\ub0b4\uc6a9": "3. \uc774 \ubc95 \ub610\ub294 \ub2e4\ub978 \ubc95\ub839\uc5d0 \ub530\ub978 \uc81c\ud55c\uc5d0 \uc704\ubc30\ub418\ub294 \uacbd\uc6b0"}, {"\ud638\ubc88\ud638": "4.", "\ud638\ub0b4\uc6a9": "4. \ud574\ub2f9 \uc0ac\ub3c4\uc758 \uac1c\uc124\u318d\uac1c\ucd95\u318d\uc99d\ucd95 \ub610\ub294 \ubcc0\uacbd\uc73c\ub85c \uc778\ud558\uc5ec \uc8fc\ubcc0\uc5d0 \uac70\uc8fc\ud558\ub294 \uc8fc\ubbfc\uc758 \uc0ac\uc0dd\ud65c \ub4f1 \uc8fc\uac70\ud658\uacbd\uc744 \uc2ec\uac01\ud558\uac8c \uce68\ud574\ud558\uac70\ub098 \uc0ac\ub78c\uc758 \ud1b5\ud589\uc5d0 \uc704\ud5d8\uc744 \uac00\uc838\uc62c \uac83\uc73c\ub85c \uc778\uc815\ub418\ub294 \uacbd\uc6b0"}]}, {"\ud56d\ubc88\ud638": "\u2463", "\ud56d\ub0b4\uc6a9": "\u2463 \uc2dc\uc7a5\u318d\uad70\uc218\u318d\uad6c\uccad\uc7a5\uc740 \uc81c1\ud56d\uc5d0 \ub530\ub978 \ud5c8\uac00\ub97c \ud558\uc600\uc744 \ub54c\uc5d0\ub294 \uc9c0\uccb4 \uc5c6\uc774 \uadf8 \ub0b4\uc6a9\uc744 \uacf5\ubcf4\uc5d0 \uace0\uc2dc\ud558\uace0, \uad6d\ud1a0\uad50\ud1b5\ubd80\ub839\