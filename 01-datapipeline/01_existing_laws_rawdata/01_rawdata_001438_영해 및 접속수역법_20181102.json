{"root": {"\ubc95\ub839": {"@\ubc95\ub839\ud0a4": "0014382018031315429", "\uae30\ubcf8\uc815\ubcf4": {"\ubc95\ub839ID": "001438", "\uacf5\ud3ec\uc77c\uc790": "20180313", "\uacf5\ud3ec\ubc88\ud638": "15429", "\uc5b8\uc5b4": "\ud55c\uae00", "\ubc95\uc885\uad6c\ubd84": {"@\ubc95\uc885\uad6c\ubd84\ucf54\ub4dc": "A0002", "#text": "\ubc95\ub960"}, "\ubc95\ub839\uba85_\ud55c\uae00": "\uc601\ud574 \ubc0f \uc811\uc18d\uc218\uc5ed\ubc95", "\ubc95\ub839\uba85_\ud55c\uc790": "\uc601\ud574 \ubc0f \uc811\uc18d\uc218\uc5ed\ubc95", "\ubc95\ub839\uba85\uc57d\uce6d": "\uc601\ud574\ubc95", "\uc81c\uba85\ubcc0\uacbd\uc5ec\ubd80": "N", "\ud55c\uae00\ubc95\ub839\uc5ec\ubd80": "N", "\ud3b8\uc7a5\uc808\uad00": "01020100", "\uc18c\uad00\ubd80\ucc98": {"@\uc18c\uad00\ubd80\ucc98\ucf54\ub4dc": "1262000", "#text": "\uc678\uad50\ubd80"}, "\uc2dc\ud589\uc77c\uc790": "20180614", "\uc81c\uac1c\uc815\uad6c\ubd84": "\uc77c\ubd80\uac1c\uc815", "\ubcc4\ud45c\ud3b8\uc9d1\uc5ec\ubd80": "N", "\uacf5\ud3ec\ubc95\ub839\uc5ec\ubd80": "Y"}, "\uc870\ubb38": {"\uc870\ubb38\ub2e8\uc704": [{"@\uc870\ubb38\ud0a4": "0001001", "\uc870\ubb38\ubc88\ud638": "1", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uc601\ud574\uc758 \ubc94\uc704", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20180614", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c1\uc870(\uc601\ud574\uc758 \ubc94\uc704) \ub300\ud55c\ubbfc\uad6d\uc758 \uc601\ud574\ub294 \uae30\uc120(\u57fa\u7dda)\uc73c\ub85c\ubd80\ud130 \uce21\uc815\ud558\uc5ec \uadf8 \ubc14\uae65\ucabd 12\ud574\ub9ac\uc758 \uc120\uae4c\uc9c0\uc5d0 \uc774\ub974\ub294 \uc218\uc5ed(\u6c34\u57df)\uc73c\ub85c \ud55c\ub2e4. \ub2e4\ub9cc, \ub300\ud1b5\ub839\ub839\uc73c\ub85c \uc815\ud558\ub294 \ubc14\uc5d0 \ub530\ub77c \uc77c\uc815\uc218\uc5ed\uc758 \uacbd\uc6b0\uc5d0\ub294 12\ud574\ub9ac \uc774\ub0b4\uc5d0\uc11c \uc601\ud574\uc758 \ubc94\uc704\ub97c \ub530\ub85c \uc815\ud560 \uc218 \uc788\ub2e4.", "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2011.4.4]"}, {"@\uc870\ubb38\ud0a4": "0002001", "\uc870\ubb38\ubc88\ud638": "2", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uae30\uc120", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20180614", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c2\uc870(\uae30\uc120)", "\ud56d": [{"\ud56d\ubc88\ud638": "\u2460", "\ud56d\ub0b4\uc6a9": "\u2460 \uc601\ud574\uc758 \ud3ed\uc744 \uce21\uc815\ud558\uae30 \uc704\ud55c \ud1b5\uc0c1\uc758 \uae30\uc120\uc740 \ub300\ud55c\ubbfc\uad6d\uc774 \uacf5\uc2dd\uc801\uc73c\ub85c \uc778\uc815\ud55c \ub300\ucd95\ucc99\ud574\ub3c4(\u5927\u7e2e\u5c3a\u6d77\u5716)\uc5d0 \ud45c\uc2dc\ub41c \ud574\uc548\uc758 \uc800\uc870\uc120(\u4f4e\u6f6e\u7dda)\uc73c\ub85c \ud55c\ub2e4."}, {"\ud56d\ubc88\ud638": "\u2461", "\ud56d\ub0b4\uc6a9": "\u2461 \uc9c0\ub9ac\uc801 \ud2b9\uc218\uc0ac\uc815\uc774 \uc788\ub294 \uc218\uc5ed\uc758 \uacbd\uc6b0\uc5d0\ub294 \ub300\ud1b5\ub839\ub839\uc73c\ub85c \uc815\ud558\ub294 \uae30\uc810\uc744 \uc5f0\uacb0\ud558\ub294 \uc9c1\uc120\uc744 \uae30\uc120\uc73c\ub85c \ud560 \uc218 \uc788\ub2e4."}], "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2011.4.4]"}, {"@\uc870\ubb38\ud0a4": "0003001", "\uc870\ubb38\ubc88\ud638": "3", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\ub0b4\uc218", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20180614", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c3\uc870(\ub0b4\uc218) \uc601\ud574\uc758 \ud3ed\uc744 \uce21\uc815\ud558\uae30 \uc704\ud55c \uae30\uc120\uc73c\ub85c\ubd80\ud130 \uc721\uc9c0 \ucabd\uc5d0 \uc788\ub294 \uc218\uc5ed\uc740 \ub0b4\uc218(\u5167\u6c34)\ub85c \ud55c\ub2e4.", "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2011.4.4]"}, {"@\uc870\ubb38\ud0a4": "0003021", "\uc870\ubb38\ubc88\ud638": "3", "\uc870\ubb38\uac00\uc9c0\ubc88\ud638": "2", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uc811\uc18d\uc218\uc5ed\uc758 \ubc94\uc704", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20180614", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c3\uc870\uc7582(\uc811\uc18d\uc218\uc5ed\uc758 \ubc94\uc704) \ub300\ud55c\ubbfc\uad6d\uc758 \uc811\uc18d\uc218\uc5ed\uc740 \uae30\uc120\uc73c\ub85c\ubd80\ud130 \uce21\uc815\ud558\uc5ec \uadf8 \ubc14\uae65\ucabd 24\ud574\ub9ac\uc758 \uc120\uae4c\uc9c0\uc5d0 \uc774\ub974\ub294 \uc218\uc5ed\uc5d0\uc11c \ub300\ud55c\ubbfc\uad6d\uc758 \uc601\ud574\ub97c \uc81c\uc678\ud55c \uc218\uc5ed\uc73c\ub85c \ud55c\ub2e4. \ub2e4\ub9cc, \ub300\ud1b5\ub839\ub839\uc73c\ub85c \uc815\ud558\ub294 \ubc14\uc5d0 \ub530\ub77c \uc77c\uc815\uc218\uc5ed\uc758 \uacbd\uc6b0\uc5d0\ub294 \uae30\uc120\uc73c\ub85c\ubd80\ud130 24\ud574\ub9ac \uc774\ub0b4\uc5d0\uc11c \uc811\uc18d\uc218\uc5ed\uc758 \ubc94\uc704\ub97c \ub530\ub85c \uc815\ud560 \uc218 \uc788\ub2e4.", "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2011.4.4]"}, {"@\uc870\ubb38\ud0a4": "0004001", "\uc870\ubb38\ubc88\ud638": "4", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uc778\uc811\uad6d \ub610\ub294 \ub300\ud5a5\uad6d\uacfc\uc758 \uacbd\uacc4\uc120", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20180614", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c4\uc870(\uc778\uc811\uad6d \ub610\ub294 \ub300\ud5a5\uad6d\uacfc\uc758 \uacbd\uacc4\uc120) \ub300\ud55c\ubbfc\uad6d\uacfc \uc778\uc811\ud558\uac70\ub098 \ub9c8\uc8fc \ubcf4\uace0 \uc788\ub294 \uad6d\uac00\uc640\uc758 \uc601\ud574 \ubc0f \uc811\uc18d\uc218\uc5ed\uc758 \uacbd\uacc4\uc120\uc740 \uad00\uacc4\uad6d\uacfc \ubcc4\ub3c4\uc758 \ud569\uc758\uac00 \uc5c6\uc73c\uba74 \ub450 \ub098\ub77c\uac00 \uac01\uc790 \uc601\ud574\uc758 \ud3ed\uc744 \uce21\uc815\ud558\ub294 \uae30\uc120\uc0c1\uc758 \uac00\uc7a5 \uac00\uae4c\uc6b4 \uc9c0\uc810\uc73c\ub85c\ubd80\ud130 \uac19\uc740 \uac70\ub9ac\uc5d0 \uc788\ub294 \ubaa8\ub4e0 \uc810\uc744 \uc5f0\uacb0\ud558\ub294 \uc911\uac04\uc120\uc73c\ub85c \ud55c\ub2e4.", "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2011.4.4]"}, {"@\uc870\ubb38\ud0a4": "0005001", "\uc870\ubb38\ubc88\ud638": "5", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uc678\uad6d\uc120\ubc15\uc758 \ud1b5\ud56d", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20180614", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c5\uc870(\uc678\uad6d\uc120\ubc15\uc758 \ud1b5\ud56d)", "\ud56d": [{"\ud56d\ubc88\ud638": "\u2460", "\ud56d\ub0b4\uc6a9": "\u2460 \uc678\uad6d\uc120\ubc15\uc740 \ub300\ud55c\ubbfc\uad6d\uc758 \ud3c9\ud654\u318d\uacf5\uacf5\uc9c8\uc11c \ub610\ub294 \uc548\uc804\ubcf4\uc7a5\uc744 \ud574\uce58\uc9c0 \uc544\ub2c8\ud558\ub294 \ubc94\uc704\uc5d0\uc11c \ub300\ud55c\ubbfc\uad6d\uc758 \uc601\ud574\ub97c \ubb34\ud574\ud1b5\ud56d(\u7121\u5bb3\u901a\u822a)\ud560 \uc218 \uc788\ub2e4. \uc678\uad6d\uc758 \uad70\ud568 \ub610\ub294 \ube44\uc0c1\uc5c5\uc6a9 \uc815\ubd80\uc120\ubc15\uc774 \uc601\ud574\ub97c \ud1b5\ud56d\ud558\ub824\ub294 \uacbd\uc6b0\uc5d0\ub294 \ub300\ud1b5\ub839\ub839\uc73c\ub85c \uc815\ud558\ub294 \ubc14\uc5d0 \ub530\ub77c \uad00\uacc4 \ub2f9\uad6d\uc5d0 \ubbf8\ub9ac \uc54c\ub824\uc57c \ud55c\ub2e4."}, {"\ud56d\ubc88\ud638": "\u2461", "\ud56d\ub0b4\uc6a9": "\u2461 \uc678\uad6d\uc120\ubc15\uc774 \ud1b5\ud56d\ud560 \ub54c \ub2e4\uc74c \uac01 \ud638\uc758 \ud589\uc704\ub97c \ud558\ub294 \uacbd\uc6b0\uc5d0\ub294 \ub300\ud55c\ubbfc\uad6d\uc758 \ud3c9\ud654\u318d\uacf5\uacf5\uc9c8\uc11c \ub610\ub294 \uc548\uc804\ubcf4\uc7a5\uc744 \ud574\uce58\ub294 \uac83\uc73c\ub85c \ubcf8\ub2e4. \ub2e4\ub9cc, \uc81c2\ud638\ubd80\ud130 \uc81c5\ud638\uae4c\uc9c0, \uc81c11\ud638 \ubc0f \uc81c13\ud638\uc758 \ud589\uc704\ub85c\uc11c \uad00\uacc4 \ub2f9\uad6d\uc758 \ud5c8\uac00\u318d\uc2b9\uc778 \ub610\ub294 \ub3d9\uc758\ub97c \ubc1b\uc740 \uacbd\uc6b0\uc5d0\ub294 \uadf8\ub7ec\ud558\uc9c0 \uc544\ub2c8\ud558\ub2e4.", "\ud638": [{"\ud638\ubc88\ud638": "1.", "\ud638\ub0b4\uc6a9": "1. \ub300\ud55c\ubbfc\uad6d\uc758 \uc8fc\uad8c\u318d\uc601\ud1a0\ubcf4\uc804 \ub610\ub294 \ub3c5\ub9bd\uc5d0 \ub300\ud55c \uc5b4\ub5a0\ud55c \ud798\uc758 \uc704\ud611\uc774\ub098 \ud589\uc0ac(\u884c\u4f7f), \uadf8 \ubc16\uc5d0 \uad6d\uc81c\uc5f0\ud569\ud5cc\uc7a5\uc5d0 \uad6c\ud604\ub41c \uad6d\uc81c\ubc95\uc6d0\uce59\uc744 \uc704\ubc18\ud55c \ubc29\ubc95\uc73c\ub85c \ud558\ub294 \uc5b4\ub5a0\ud55c \ud798\uc758 \uc704\ud611\uc774\ub098 \ud589\uc0ac"}, {"\ud638\ubc88\ud638": "2.", "\ud638\ub0b4\uc6a9": "2. \ubb34\uae30\ub97c \uc0ac\uc6a9\ud558\uc5ec \ud558\ub294 \ud6c8\ub828 \ub610\ub294 \uc5f0\uc2b5"}, {"\ud638\ubc88\ud638": "3.", "\ud638\ub0b4\uc6a9": "3. \ud56d\uacf5\uae30\uc758 \uc774\ud568(\uf9ea\u8266)\u318d\ucc29\ud568(\u7740\u8266) \ub610\ub294 \ud0d1\uc7ac"}, {"\ud638\ubc88\ud638": "4.", "\ud638\ub0b4\uc6a9": "4. \uad70\uc0ac\uae30\uae30\uc758 \ubc1c\uc9c4(\u767c\u9032)\u318d\ucc29\ud568 \ub610\ub294 \ud0d1\uc7ac"}, {"\ud638\ubc88\ud638": "5.", "\ud638\ub0b4\uc6a9": "5. \uc7a0\uc218\ud56d\ud589"}, {"\ud638\ubc88\ud638": "6.", "\ud638\ub0b4\uc6a9": "6. \ub300\ud55c\ubbfc\uad6d\uc758 \uc548\uc804\ubcf4\uc7a5\uc5d0 \uc720\ud574\ud55c \uc815\ubcf4\uc758 \uc218\uc9d1"}, {"\ud638\ubc88\ud638": "7.", "\ud638\ub0b4\uc6a9": "7. \ub300\ud55c\ubbfc\uad6d\uc758 \uc548\uc804\ubcf4\uc7a5\uc5d0 \uc720\ud574\ud55c \uc120\uc804\u318d\uc120\ub3d9"}, {"\ud638\ubc88\ud638": "8.", "\ud638\ub0b4\uc6a9": "8. \ub300\ud55c\ubbfc\uad6d\uc758 \uad00\uc138\u318d\uc7ac\uc815\u318d\ucd9c\uc785\uad6d\uad00\ub9ac \ub610\ub294 \ubcf4\uac74\u318d\uc704\uc0dd\uc5d0 \uad00\ud55c \ubc95\uaddc\uc5d0 \uc704\ubc18\ub418\ub294 \ubb3c\ud488\uc774\ub098 \ud1b5\ud654(\u901a\u8ca8)\uc758 \uc591\ud558(\u63da\u8377)\u318d\uc801\ud558(\u7a4d\u8377) \ub610\ub294 \uc0ac\ub78c\uc758 \uc2b9\uc120\u318d\ud558\uc120"}, {"\ud638\ubc88\ud638": "9.", "\ud638\ub0b4\uc6a9": "9. \ub300\ud1b5\ub839\ub839\uc73c\ub85c \uc815\ud558\ub294 \uae30\uc900\uc744 \ucd08\uacfc\ud558\ub294 \uc624\uc5fc\ubb3c\uc9c8\uc758 \ubc30\ucd9c"}, {"\ud638\ubc88\ud638": "10.", "\ud638\ub0b4\uc6a9": "10. \uc5b4\ub85c(\u6f01\u6488)"}, {"\ud638\ubc88\ud638": "11.", "\ud638\ub0b4\uc6a9": "11. \uc870\uc0ac \ub610\ub294 \uce21\ub7c9"}, {"\ud638\ubc88\ud638": "12.", "\ud638\ub0b4\uc6a9": "12. \ub300\ud55c\ubbfc\uad6d \ud1b5\uc2e0\uccb4\uc81c\uc758 \ubc29\ud574 \ub610\ub294 \uc124\ube44 \ubc0f \uc2dc\uc124\ubb3c\uc758 \ud6fc\uc190"}, {"\ud638\ubc88\ud638": "13.", "\ud638\ub0b4\uc6a9": "13. \ud1b5\ud56d\uacfc \uc9c1\uc811 \uad00\ub828 \uc5c6\ub294 \ud589\uc704\ub85c\uc11c \ub300\ud1b5\ub839\ub839\uc73c\ub85c \uc815\ud558\ub294 \uac83"}]}, {"\ud56d\ubc88\ud638": "\u2462", "\ud56d\ub0b4\uc6a9": "\u2462 \ub300\ud55c\ubbfc\uad6d\uc758 \uc548\uc804\ubcf4\uc7a5\uc744 \uc704\ud558\uc5ec \ud544\uc694\ud558\ub2e4\uace0 \uc778\uc815\ub418\ub294 \uacbd\uc6b0\uc5d0\ub294 \ub300\ud1b5\ub839\ub839\uc73c\ub85c \uc815\ud558\ub294 \ubc14\uc5d0 \ub530\ub77c \uc77c\uc815\uc218\uc5ed\uc744 \uc815\ud558\uc5ec \uc678\uad6d\uc120\ubc15\uc758 \ubb34\ud574\ud1b5\ud56d\uc744 \uc77c\uc2dc\uc801\uc73c\ub85c \uc815\uc9c0\uc2dc\ud0ac \uc218 \uc788\ub2e4."}], "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2011.4.4]"}, {"@\uc870\ubb38\ud0a4": "0006001", "\uc870\ubb38\ubc88\ud638": "6", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uc815\uc120 \ub4f1", "\uc870\ubb