\ub294 \uc0ac\ud56d\uc5d0 \ub300\ud558\uc5ec\ub294 \uc0ac\uc7a5\uc774 \uacf5\uc0ac\ub97c \ub300\ud45c\ud558\uc9c0 \ubabb\ud558\uace0, \uac10\uc0ac\uac00 \uacf5\uc0ac\ub97c \ub300\ud45c\ud55c\ub2e4.", "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2009.1.7]"}, {"@\uc870\ubb38\ud0a4": "0008001", "\uc870\ubb38\ubc88\ud638": "8", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\ub300\ub9ac\uc778\uc758 \uc120\uc784", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20180417", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c8\uc870(\ub300\ub9ac\uc778\uc758 \uc120\uc784) \uc0ac\uc7a5\uc740 \uc815\uad00\uc73c\ub85c \uc815\ud558\ub294 \ubc14\uc5d0 \ub530\ub77c \uc9c1\uc6d0 \uc911\uc5d0\uc11c \uacf5\uc0ac\uc758 \uc5c5\ubb34\uc5d0 \uad00\ud55c \uc7ac\ud310\uc0c1 \ub610\ub294 \uc7ac\ud310 \uc678\uc758 \ubaa8\ub4e0 \ud589\uc704\ub97c \ud560 \uc218 \uc788\ub294 \uad8c\ud55c\uc744 \uac00\uc9c4 \ub300\ub9ac\uc778\uc744 \uc120\uc784\ud560 \uc218 \uc788\ub2e4.", "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2009.1.7]"}, {"@\uc870\ubb38\ud0a4": "0009001", "\uc870\ubb38\ubc88\ud638": "9", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\ube44\ubc00\ub204\uc124 \uae08\uc9c0 \ub4f1", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20180417", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c9\uc870(\ube44\ubc00\ub204\uc124 \uae08\uc9c0 \ub4f1) \uacf5\uc0ac\uc758 \uc784\uc9c1\uc6d0\uc774\ub098 \uadf8 \uc9c1\uc5d0 \uc788\uc5c8\ub358 \uc790\ub294 \uc9c1\ubb34\uc0c1 \uc54c\uac8c \ub41c \ube44\ubc00\uc744 \ub204\uc124\ud558\uac70\ub098 \ub3c4\uc6a9(\u76dc\u7528)\ud558\uc5ec\uc11c\ub294 \uc544\ub2c8 \ub41c\ub2e4.", "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2009.1.7]"}, {"@\uc870\ubb38\ud0a4": "0010001", "\uc870\ubb38\ubc88\ud638": "10", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uc0ac\uc5c5", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20180417", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": "Y", "\uc870\ubb38\ub0b4\uc6a9": "\uc81c10\uc870(\uc0ac\uc5c5)", "\ud56d": [{"\ud56d\ubc88\ud638": "\u2460", "\ud56d\uc81c\uac1c\uc815\uc720\ud615": "\uac1c\uc815", "\ud56d\uc81c\uac1c\uc815\uc77c\uc790\ubb38\uc790\uc5f4": "2011.3.30, 2013.3.23, 2014.1.21, 2018.4.17", "\ud56d\ub0b4\uc6a9": "\u2460 \uacf5\uc0ac\ub294 \uc81c1\uc870\uc758 \ubaa9\uc801\uc744 \ub2ec\uc131\ud558\uae30 \uc704\ud558\uc5ec \ub2e4\uc74c \uac01 \ud638\uc758 \uc0ac\uc5c5\uc744 \ud55c\ub2e4. <\uac1c\uc815 2011.3.30, 2013.3.23, 2014.1.21, 2018.4.17>", "\ud638": [{"\ud638\ubc88\ud638": "1.", "\ud638\ub0b4\uc6a9": "1. \ubb34\uc5ed \uc9c4\ud765\uacfc \uc678\uad6d\uc778\ud22c\uc790 \uc720\uce58\ub97c \uc704\ud55c \ud574\uc678\uc2dc\uc7a5\uc758 \uc870\uc0ac\u318d\uac1c\ucc99 \ubc0f \uc815\ubcf4\uc758 \uc218\uc9d1\uacfc \uadf8 \uc131\uacfc\uc758 \ubcf4\uae09"}, {"\ud638\ubc88\ud638": "2.", "\ud638\ub0b4\uc6a9": "2. \uad6d\ub0b4\uc758 \uc0b0\uc5c5\u318d\uc0c1\ud488\uacfc \uc678\uad6d\uc778\ud22c\uc790 \ud658\uacbd\uc758 \ud574\uc678 \ud64d\ubcf4 \ubc0f \uad6d\uac00\ube0c\ub79c\ub4dc \uc81c\uace0 \uad00\ub828 \uc9c0\uc6d0"}, {"\ud638\ubc88\ud638": "3.", "\ud638\ub0b4\uc6a9": "3. \ubb34\uc5ed\uac70\ub798, \uad6d\ub0b4\uc678 \uae30\uc5c5 \uac04 \ud22c\uc790 \ud611\ub825\uacfc \uc0b0\uc5c5\uae30\uc220 \uad50\ub958\uc758 \uc54c\uc120 \ubc0f \u300c\uad6d\uc81c\uac1c\ubc1c\ud611\ub825\uae30\ubcf8\ubc95\u300d \uc81c2\uc870\uc81c1\ud638\uc5d0 \ub530\ub978 \uad6d\uc81c\uac1c\ubc1c\ud611\ub825 \uc9c0\uc6d0"}, {"\ud638\ubc88\ud638": "4.", "\ud638\ub0b4\uc6a9": "4. \ubb34\uc5ed \ubc0f \ud22c\uc790\uc5d0 \uad00\ud55c \ubc15\ub78c\ud68c\u318d\uc804\uc2dc\ud68c\uc758 \uac1c\ucd5c \ub610\ub294 \ucc38\uac00 \ubc0f \ucc38\uac00\uc758 \uc54c\uc120"}, {"\ud638\ubc88\ud638": "5.", "\ud638\ub0b4\uc6a9": "5. \uc0b0\uc5c5\ud1b5\uc0c1\uc790\uc6d0\ubd80\uc7a5\uad00\uc774 \uc815\ud558\ub294 \uc218\ucd9c \ub610\ub294 \uc218\uc785"}, {"\ud638\ubc88\ud638": "6.", "\ud638\ub0b4\uc6a9": "6. \uc678\uad6d\uc778\ud22c\uc790\uc758 \uc720\uce58 \ubc0f \uad6d\ub0b4\uae30\uc5c5\uc758 \ud574\uc678\ud22c\uc790(\ud574\uc678 \uc790\uc6d0\u318d\uc5d0\ub108\uc9c0 \uac1c\ubc1c\uc744 \ud3ec\ud568\ud55c\ub2e4) \uc9c0\uc6d0"}, {"\ud638\ubc88\ud638": "7.", "\ud638\ub0b4\uc6a9": "7. \uad6d\uc81c \uacbd\uc7c1\ub825 \uac15\ud654\ub97c \uc704\ud55c \ud574\uc678 \uc804\ubb38\uc778\ub825\uc758 \uc720\uce58 \uc9c0\uc6d0 \ubc0f \uad6d\ub0b4 \uc804\ubb38\uc778\ub825\uc758 \ud574\uc678 \ucc3d\uc5c5\u318d\ucde8\uc5c5 \uc9c0\uc6d0"}, {"\ud638\ubc88\ud638": "8.", "\ud638\ub0b4\uc6a9": "8. \u300c\ubc29\uc704\uc0ac\uc5c5\ubc95\u300d \uc81c38\uc870\uc81c1\ud56d\uc81c4\ud638\uc5d0 \ub530\ub978 \ubc29\uc0b0\ubb3c\uc790\uc640 \ubc29\uc0b0\ubb3c\uc790\uc5d0 \uc900\ud558\ub294 \ubb3c\uc790(\uc774\ud558 \"\ubc29\uc0b0\ubb3c\uc790\ub4f1\"\uc774\ub77c \ud55c\ub2e4)\uc758 \uc218\ucd9c\uacfc \uad00\ub828\ud55c \ub2e4\uc74c \uac01 \ubaa9\uc758 \uc0ac\uc5c5", "\ubaa9": [{"\ubaa9\ubc88\ud638": "\uac00.", "\ubaa9\ub0b4\uc6a9": "\uac00. \uad6d\ub0b4 \uae30\uc5c5\uc744 \ub300\uc2e0\ud55c \uad6c\ub9e4\uad6d\uc815\ubd80\uc640\uc758 \ubc29\uc0b0\ubb3c\uc790\ub4f1 \uc218\ucd9c\uc5d0 \uad00\ud55c \uacc4\uc57d \uc2dc \ub2f9\uc0ac\uc790\uc9c0\uc704 \uc218\ud589"}, {"\ubaa9\ubc88\ud638": "\ub098.", "\ubaa9\ub0b4\uc6a9": "\ub098. \ubc29\uc0b0\ubb3c\uc790\ub4f1\uacfc \uc0b0\uc5c5\u318d\uc790\uc6d0 \ubc0f \ud22c\uc790 \ud611\ub825\uc744 \uc5f0\uacc4\ud55c \ud328\ud0a4\uc9c0 \ud611\uc0c1\uc548\uc758 \uc791\uc131\uacfc \uae08\uc735\uc9c0\uc6d0\ubc29\uc548 \uc218\ub9bd"}, {"\ubaa9\ubc88\ud638": "\ub2e4.", "\ubaa9\ub0b4\uc6a9": "\ub2e4. \uadf8 \ubc16\uc5d0 \ubc29\uc0b0\ubb3c\uc790\ub4f1\uc758 \uad50\uc5ed\uc9c0\uc6d0\uc744 \uc704\ud558\uc5ec \uc0b0\uc5c5\ud1b5\uc0c1\uc790\uc6d0\ubd80\uc7a5\uad00 \ubc0f \ubc29\uc704\uc0ac\uc5c5\uccad\uc7a5\uc774 \ud544\uc694\ud558\ub2e4\uace0 \uc778\uc815\ud558\ub294 \uc5c5\ubb34"}]}, {"\ud638\ubc88\ud638": "9.", "\ud638\ub0b4\uc6a9": "9. \u300c\ub300\uc678\ubb34\uc5ed\ubc95\u300d \uc81c32\uc870\uc7583\uc81c2\ud56d\uc5d0 \ub530\ub978 \uc815\ubd80\uac04 \uc218\ucd9c\uacc4\uc57d \uad00\ub828 \uc0ac\uc5c5"}, {"\ud638\ubc88\ud638": "9.", "\ud638\uac00\uc9c0\ubc88\ud638": "2", "\ud638\ub0b4\uc6a9": "9\uc7582. \ud574\uc678 \ud55c\uc778\uacbd\uc81c\uc778 \ub124\ud2b8\uc6cc\ud06c \uad6c\ucd95\u318d\uad00\ub9ac \ubc0f \ud65c\uc6a9 \uc9c0\uc6d0"}, {"\ud638\ubc88\ud638": "10.", "\ud638\ub0b4\uc6a9": "10. \uc2dc\uc124\uc758 \uc6b4\uc601, \uc804\ubb38\uc778\ub825\uc758 \uad50\uc721\u318d\ud6c8\ub828 \ubc0f \uc721\uc131 \ub4f1 \uc81c1\ud638\ubd80\ud130 \uc81c9\ud638\uae4c\uc9c0 \ubc0f \uc81c9\ud638\uc7582\uc758 \uc0ac\uc5c5\uc5d0 \ub538\ub9b0 \uc0ac\uc5c5"}, {"\ud638\ubc88\ud638": "11.", "\ud638\ub0b4\uc6a9": "11. \ub2e4\ub978 \ubc95\ub960\uc5d0 \ub530\ub77c \uacf5\uc0ac\uac00 \ud560 \uc218 \uc788\ub294 \uc0ac\uc5c5"}]}, {"\ud56d\ubc88\ud638": "\u2461", "\ud56d\ub0b4\uc6a9": "\u2461 \uacf5\uc0ac\ub294 \uc81c1\ud56d\uc81c8\ud638\uc758 \uc5c5\ubb34\ub97c \ud6a8\uacfc\uc801\uc73c\ub85c \uc218\ud589\ud558\uae30 \uc704\ud558\uc5ec \ubc29\uc0b0\ubb3c\uc790\uad50\uc5ed\uc9c0\uc6d0\uc13c\ud130\ub97c \ub454\ub2e4. <\uc2e0\uc124 2011.3.30>"}], "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2009.1.7]"}, {"@\uc870\ubb38\ud0a4": "0011001", "\uc870\ubb38\ubc88\ud638": "11", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uacbd\ube44 \ubc0f \uc218\uc218\ub8cc\uc758 \ubd80\ub2f4", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20180417", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c11\uc870(\uacbd\ube44 \ubc0f \uc218\uc218\ub8cc\uc758 \ubd80\ub2f4) \uacf5\uc0ac\ub294 \uc81c10\uc870\uc81c1\ud56d \uac01 \ud638\uc758 \uc0ac\uc5c5\uc5d0 \ub4e4\uc5b4\uac00\ub294 \uacbd\ube44\uc640 \uc218\uc218\ub8cc\ub97c \uc218\uc775\uc790\uc5d0\uac8c \ubd80\ub2f4\ud558\uac8c \ud560 \uc218 \uc788\ub2e4. <\uac1c\uc815 2011.3.30>", "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2009.1.7]"}, {"@\uc870\ubb38\ud0a4": "0012001", "\uc870\ubb38\ubc88\ud638": "12", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uc190\uc775\uae08\uc758 \ucc98\ub9ac", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20180417", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c12\uc870(\uc190\uc775\uae08\uc758 \ucc98\ub9ac)", "\ud56d": [{"\ud56d\ubc88\ud638": "\u2460", "\ud56d\ub0b4\uc6a9": "\u2460 \uacf5\uc0ac\ub294 \ub9e4 \uc0ac\uc5c5\uc5f0\ub3c4\ub97c \uacb0\uc0b0\ud55c \uacb0\uacfc \uc774\uc775\uc774 \uc0dd\uacbc\uc744 \uacbd\uc6b0\uc5d0\ub294 \ub2e4\uc74c \uac01 \ud638\uc758 \uc21c\uc11c\ub300\ub85c \ucc98\ub9ac\ud55c\ub2e4. <\uac1c\uc815 2012.5.23>", "\ud638": [{"\ud638\ubc88\ud638": "1.", "\ud638\ub0b4\uc6a9": "1. \uc774\uc6d4\uacb0\uc190\uae08\uc758 \ubcf4\uc804(\u88dc\u5861)"}, {"\ud638\ubc88\ud638": "2.", "\ud638\ub0b4\uc6a9": "2. \uc790\ubcf8\uae08\uacfc \uac19\uc740 \uae08\uc561\uc5d0 \uc774\ub97c \ub54c\uae4c\uc9c0 \uc774\uc775\uae08\uc758 10\ubd84\uc758 2 \uc774\uc0c1\uc744 \uc774\uc775\uc900\ube44\uae08\uc73c\ub85c \uc801\ub9bd"}, {"\ud638\ubc88\ud638": "3.", "\ud638\ub0b4\uc6a9": "3. \uc81c2\ud638\uc5d0 \ub530\ub978 \uc774\uc775\uc900\ube44\uae08 \uc678\uc758 \uc900\ube44\uae08\uc73c\ub85c \uc801\ub9bd"}, {"\ud638\ubc88\ud638": "4.", "\ud638\ub0b4\uc6a9": "4. \uad6d\uace0\uc5d0 \ub0a9\uc785"}]}, {"\ud56d\ubc88\ud638": "\u2461", "\ud56d\ub0b4\uc6a9": "\u2461 \uacf5\uc0ac\ub294 \ub9e4 \uc0ac\uc5c5\