\ud638": "8", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\ub300\ub9ac\uc778\uc758 \uc120\uc784", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20151223", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c8\uc870(\ub300\ub9ac\uc778\uc758 \uc120\uc784) \uc0ac\uc7a5\uc740 \uc815\uad00\uc73c\ub85c \uc815\ud558\ub294 \ubc14\uc5d0 \ub530\ub77c \uacf5\uc0ac\uc758 \uc9c1\uc6d0 \uc911\uc5d0\uc11c \uacf5\uc0ac\uc758 \uc5c5\ubb34\uc5d0 \uad00\ud55c \uc7ac\ud310\uc0c1 \ub610\ub294 \uc7ac\ud310 \uc678\uc758 \ubaa8\ub4e0 \ud589\uc704\ub97c \ud560 \uc218 \uc788\ub294 \uad8c\ud55c\uc744 \uac00\uc9c0\ub294 \ub300\ub9ac\uc778\uc744 \uc120\uc784\ud560 \uc218 \uc788\ub2e4.", "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2009.3.5]"}, {"@\uc870\ubb38\ud0a4": "0009001", "\uc870\ubb38\ubc88\ud638": "9", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\ube44\ubc00\ub204\uc124 \uae08\uc9c0 \ub4f1", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20151223", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c9\uc870(\ube44\ubc00\ub204\uc124 \uae08\uc9c0 \ub4f1) \uacf5\uc0ac\uc758 \uc784\uc9c1\uc6d0\uc774\uac70\ub098 \uadf8 \uc9c1\uc5d0 \uc788\uc5c8\ub358 \uc0ac\ub78c\uc740 \uc9c1\ubb34\uc0c1 \uc54c\uac8c \ub41c \ube44\ubc00\uc744 \ub204\uc124\ud558\uac70\ub098 \ub3c4\uc6a9(\u76dc\u7528)\ud558\uc5ec\uc11c\ub294 \uc544\ub2c8 \ub41c\ub2e4.", "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2009.3.5]"}, {"@\uc870\ubb38\ud0a4": "0010001", "\uc870\ubb38\ubc88\ud638": "10", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uc0ac\uc5c5", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20151223", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": "Y", "\uc870\ubb38\ub0b4\uc6a9": "\uc81c10\uc870(\uc0ac\uc5c5)", "\ud56d": [{"\ud56d\ubc88\ud638": "\u2460", "\ud56d\uc81c\uac1c\uc815\uc720\ud615": "\uac1c\uc815", "\ud56d\uc81c\uac1c\uc815\uc77c\uc790\ubb38\uc790\uc5f4": "2011.7.25, 2015.6.22", "\ud56d\ub0b4\uc6a9": "\u2460 \uacf5\uc0ac\ub294 \uc81c1\uc870\uc758 \ubaa9\uc801\uc744 \ub2ec\uc131\ud558\uae30 \uc704\ud558\uc5ec \ub2e4\uc74c \uac01 \ud638\uc758 \uc0ac\uc5c5\uc744 \ud55c\ub2e4. <\uac1c\uc815 2011.7.25, 2015.6.22>", "\ud638": [{"\ud638\ubc88\ud638": "1.", "\ud638\ub0b4\uc6a9": "1. \ub18d\uc0b0\ubb3c\u318d\uc784\uc0b0\ubb3c\u318d\ucd95\uc0b0\ubb3c \ubc0f \uc218\uc0b0\ubb3c(\uc774\ud558 \"\ub18d\uc218\uc0b0\ubb3c\"\uc774\ub77c \ud55c\ub2e4) \uc720\ud1b5\uc0b0\uc5c5(\ub18d\uc218\uc0b0\ubb3c\uc758 \uc800\uc7a5\u318d\ucc98\ub9ac\u318d\uac00\uacf5\u318d\ud310\ub9e4 \ubc0f \uc720\ud1b5\uac1c\uc120\uc5d0 \uad00\ud55c \uc0ac\uc5c5\uc744 \ub9d0\ud55c\ub2e4. \uc774\ud558 \uac19\ub2e4)\uc5d0 \uad00\ud55c \uac74\uc124 \ubc0f \uc6b4\uc601"}, {"\ud638\ubc88\ud638": "2.", "\ud638\ub0b4\uc6a9": "2. \ub18d\uc218\uc0b0\ubb3c \uc720\ud1b5\uc0b0\uc5c5 \ubc0f \uc2dd\ud488\uc0b0\uc5c5(\u300c\ub18d\uc5c5\u318d\ub18d\ucd0c \ubc0f \uc2dd\ud488\uc0b0\uc5c5 \uae30\ubcf8\ubc95\u300d \uc81c3\uc870\uc81c8\ud638\uc5d0 \ub530\ub978 \uc2dd\ud488\uc0b0\uc5c5\uc744 \ub9d0\ud55c\ub2e4. \uc774\ud558 \uac19\ub2e4)\uc5d0 \ub300\ud55c \ud22c\uc790\uc640 \uc790\uae08\uc758 \ub300\uc5ec \ubc0f \uc54c\uc120"}, {"\ud638\ubc88\ud638": "3.", "\ud638\ub0b4\uc6a9": "3. \ub18d\uc218\uc0b0\ubb3c \uc720\ud1b5\uc0b0\uc5c5 \ubc0f \uc2dd\ud488\uc0b0\uc5c5\uc5d0 \uad00\ud55c \uae30\uc220\uc9c0\ub3c4\uc640 \uc678\uad6d\uae30\uc220\uc758 \ub3c4\uc785 \ubc0f \uc54c\uc120"}, {"\ud638\ubc88\ud638": "4.", "\ud638\ub0b4\uc6a9": "4. \ub18d\uc218\uc0b0\ubb3c \uc720\ud1b5\uc0b0\uc5c5 \ubc0f \uc2dd\ud488\uc0b0\uc5c5\uc5d0 \uad00\ud55c \uacbd\uc601\uc9c0\ub3c4"}, {"\ud638\ubc88\ud638": "5.", "\ud638\ub0b4\uc6a9": "5. \ub18d\uc218\uc0b0\ubb3c\uacfc \uadf8 \uac00\uacf5\uc81c\ud488 \ubc0f \uc2dd\ud488(\u300c\ub18d\uc5c5\u318d\ub18d\ucd0c \ubc0f \uc2dd\ud488\uc0b0\uc5c5 \uae30\ubcf8\ubc95\u300d \uc81c3\uc870\uc81c7\ud638\uc5d0 \ub530\ub978 \uc2dd\ud488\uc744 \ub9d0\ud55c\ub2e4. \uc774\ud558 \uac19\ub2e4)\uc758 \uc2dc\uc7a5\uac1c\ucc99\uacfc \uc218\ucd9c\uc785 \ubc0f \uc54c\uc120"}, {"\ud638\ubc88\ud638": "6.", "\ud638\ub0b4\uc6a9": "6. \ub18d\uc218\uc0b0\ubb3c \ubc0f \uadf8 \uac00\uacf5\uc81c\ud488\uacfc \uc2dd\ud488\uc758 \uc18c\ube44\uc5d0 \uad00\ud55c \uc870\uc0ac\u318d\uc5f0\uad6c, \uc815\ubcf4 \uc81c\uacf5, \ud310\ub9e4\ucd09\uc9c4 \ubc0f \ud64d\ubcf4\uc640 \ud488\uc9c8\uad00\ub9ac\uc5d0 \uad00\ud55c \uc0ac\uc5c5"}, {"\ud638\ubc88\ud638": "7.", "\ud638\ub0b4\uc6a9": "7. \ub18d\uc218\uc0b0\ubb3c \uc720\ud1b5\uc0b0\uc5c5 \ubc0f \uc2dd\ud488\uc0b0\uc5c5\uc5d0 \uad00\ud55c \uc870\uc0ac\u318d\uc5f0\uad6c\u318d\uad50\uc721 \ubc0f \ucee8\uc124\ud305"}, {"\ud638\ubc88\ud638": "8.", "\ud638\ub0b4\uc6a9": "8. \ub18d\uc218\uc0b0\ubb3c \ubc0f \uc2dd\ud488\uc758 \uc804\uc790\uac70\ub798(\u300c\uc804\uc790\uac70\ub798\uae30\ubcf8\ubc95\u300d \uc81c2\uc870\uc81c5\ud638\uc5d0 \ub530\ub978 \uc804\uc790\uac70\ub798\ub97c \ub9d0\ud55c\ub2e4)\uc5d0 \uad00\ud55c \uc0ac\uc5c5"}, {"\ud638\ubc88\ud638": "9.", "\ud638\ub0b4\uc6a9": "9. \uc2dd\ubb38\ud654 \ubcf4\uae09 \ubc0f \uc2dd\ud488\uc0b0\uc5c5 \uc721\uc131\u318d\uc9c4\ud765\uc5d0 \uad00\ud55c \uc0ac\uc5c5"}, {"\ud638\ubc88\ud638": "10.", "\ud638\ub0b4\uc6a9": "10. \uc218\uae09\uc870\uc808 \ubc0f \uac00\uaca9\uc548\uc815\uc744 \uc704\ud55c \ub18d\uc218\uc0b0\ubb3c\uc758 \uc218\ub9e4\u318d\ube44\ucd95 \ubc0f \ud310\ub9e4"}, {"\ud638\ubc88\ud638": "11.", "\ud638\ub0b4\uc6a9": "11. \ub0a8\ubd81 \ub18d\uc5b4\uc5c5 \ud611\ub825\uacfc \uad6d\uc678 \uc2dd\ub7c9\uc790\uc6d0 \ud655\ubcf4\uc5d0 \uad00\ud55c \uc0ac\uc5c5"}, {"\ud638\ubc88\ud638": "12.", "\ud638\ub0b4\uc6a9": "12. \uc81c1\ud638\ubd80\ud130 \uc81c11\ud638\uae4c\uc9c0\uc758 \uc0ac\uc5c5\uc5d0 \ub538\ub9b0 \uc0ac\uc5c5"}, {"\ud638\ubc88\ud638": "13.", "\ud638\ub0b4\uc6a9": "13. \ub2e4\ub978 \ubc95\ub839\uc5d0 \ub530\ub77c \uacf5\uc0ac\uac00 \ud560 \uc218 \uc788\ub294 \uc0ac\uc5c5"}, {"\ud638\ubc88\ud638": "14.", "\ud638\ub0b4\uc6a9": "14. \uadf8 \ubc16\uc5d0 \uc815\ubd80 \ub610\ub294 \uc9c0\ubc29\uc790\uce58\ub2e8\uccb4\ub85c\ubd80\ud130 \uc704\ud0c1 \ub610\ub294 \ub300\ud589\ubc1b\uc740 \uc0ac\uc5c5"}]}, {"\ud56d\ubc88\ud638": "\u2461", "\ud56d\ub0b4\uc6a9": "\u2461 \uc81c1\ud56d\uc81c1\ud638\ubd80\ud130 \uc81c4\ud638\uae4c\uc9c0 \ubc0f \uc81c7\ud638\uc5d0 \ub530\ub978 \ub18d\uc218\uc0b0\ubb3c \uc720\ud1b5\uc0b0\uc5c5\uc758 \uc885\ub958\u318d\ubc94\uc704\uc640 \uac19\uc740 \ud56d \uc81c10\ud638\uc5d0 \ub530\ub978 \uc0ac\uc5c5\uc758 \uc885\ub958\u318d\ubc94\uc704\ub294 \ub300\ud1b5\ub839\ub839\uc73c\ub85c \uc815\ud55c\ub2e4. <\uac1c\uc815 2011.7.25>"}], "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2009.3.5]"}, {"@\uc870\ubb38\ud0a4": "0011001", "\uc870\ubb38\ubc88\ud638": "11", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uc190\uc775\uae08\uc758 \ucc98\ub9ac", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20151223", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c11\uc870(\uc190\uc775\uae08\uc758 \ucc98\ub9ac)", "\ud56d": [{"\ud56d\ubc88\ud638": "\u2460", "\ud56d\ub0b4\uc6a9": "\u2460 \uacf5\uc0ac\ub294 \ub9e4 \uc0ac\uc5c5\uc5f0\ub3c4\uc758 \uacb0\uc0b0 \uacb0\uacfc \uc774\uc775\uc774 \uc0dd\uae30\uba74 \uadf8 \uc774\uc775\uc744 \ub2e4\uc74c \uac01 \ud638\uc758 \uc21c\uc11c\uc5d0 \ub530\ub77c \ucc98\ub9ac\ud55c\ub2e4.", "\ud638": [{"\ud638\ubc88\ud638": "1.", "\ud638\ub0b4\uc6a9": "1. \uc774\uc6d4\uc190\uc2e4\uae08(\u79fb\u8d8a\u640d\u5931\uf90a)\uc758 \ubcf4\uc804(\u88dc\u5861)"}, {"\ud638\ubc88\ud638": "2.", "\ud638\ub0b4\uc6a9": "2. \uc790\ubcf8\