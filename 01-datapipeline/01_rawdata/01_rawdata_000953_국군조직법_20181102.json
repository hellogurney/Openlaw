4]", "\uc870\ubb38\uac1c\uc815\ubb38": "\uc81c3\uc870\uc758 \uc81c\ubaa9 \uc911 \"\uc784\ubb34\"\ub97c \"\uc8fc\uc784\ubb34\"\ub85c \ud558\uace0, \uac19\uc740 \uc870 \uc81c2\ud56d \uc911 \"\ud574\uad70\uc740 \ud574\uc0c1\uc791\uc804\uacfc \uc0c1\ub959\uc791\uc804\uc744\"\uc744 \"\ud574\uad70\uc740 \uc0c1\ub959\uc791\uc804\uc744 \ud3ec\ud568\ud55c \ud574\uc0c1\uc791\uc804\uc744, \ud574\ubcd1\ub300\ub294 \uc0c1\ub959\uc791\uc804\uc744\"\ub85c \ud55c\ub2e4."}, {"@\uc870\ubb38\ud0a4": "0004001", "\uc870\ubb38\ubc88\ud638": "4", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uad70\uc778\uc758 \uc2e0\ubd84 \ub4f1", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20111015", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c4\uc870(\uad70\uc778\uc758 \uc2e0\ubd84 \ub4f1)", "\ud56d": [{"\ud56d\ubc88\ud638": "\u2460", "\ud56d\ub0b4\uc6a9": "\u2460 \"\uad70\uc778\"\uc774\ub780 \uc804\uc2dc\uc640 \ud3c9\uc2dc\ub97c \ub9c9\ub860\ud558\uace0 \uad70\uc5d0 \ubcf5\ubb34\ud558\ub294 \uc0ac\ub78c\uc744 \ub9d0\ud55c\ub2e4."}, {"\ud56d\ubc88\ud638": "\u2461", "\ud56d\ub0b4\uc6a9": "\u2461 \uad70\uc778\uc758 \uc778\uc0ac, \ubcd1\uc5ed \ubcf5\ubb34 \ubc0f \uc2e0\ubd84\uc5d0 \uad00\ud55c \uc0ac\ud56d\uc740 \ub530\ub85c \ubc95\ub960\ub85c \uc815\ud55c\ub2e4."}], "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2010.3.17]"}, {"@\uc870\ubb38\ud0a4": "0005001", "\uc870\ubb38\ubc88\ud638": "5", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uad70\uae30", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20111015", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c5\uc870(\uad70\uae30)", "\ud56d": [{"\ud56d\ubc88\ud638": "\u2460", "\ud56d\ub0b4\uc6a9": "\u2460 \uad6d\uad70\uc740 \uad