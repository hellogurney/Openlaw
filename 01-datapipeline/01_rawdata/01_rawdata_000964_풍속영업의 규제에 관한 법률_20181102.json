{"root": {"\ubc95\ub839": {"@\ubc95\ub839\ud0a4": "0009642016052914267", "\uae30\ubcf8\uc815\ubcf4": {"\ubc95\ub839ID": "000964", "\uacf5\ud3ec\uc77c\uc790": "20160529", "\uacf5\ud3ec\ubc88\ud638": "14267", "\uc5b8\uc5b4": "\ud55c\uae00", "\ubc95\uc885\uad6c\ubd84": {"@\ubc95\uc885\uad6c\ubd84\ucf54\ub4dc": "A0002", "#text": "\ubc95\ub960"}, "\ubc95\ub839\uba85_\ud55c\uae00": "\ud48d\uc18d\uc601\uc5c5\uc758 \uaddc\uc81c\uc5d0 \uad00\ud55c \ubc95\ub960", "\ubc95\ub839\uba85_\ud55c\uc790": "\ud48d\uc18d\uc601\uc5c5\uc758 \uaddc\uc81c\uc5d0 \uad00\ud55c \ubc95\ub960", "\ubc95\ub839\uba85\uc57d\uce6d": "\ud48d\uc18d\uc601\uc5c5\uaddc\uc81c\ubc95", "\uc81c\uba85\ubcc0\uacbd\uc5ec\ubd80": "N", "\ud55c\uae00\ubc95\ub839\uc5ec\ubd80": "N", "\ud3b8\uc7a5\uc808\uad00": "11050000", "\uc18c\uad00\ubd80\ucc98": {"@\uc18c\uad00\ubd80\ucc98\ucf54\ub4dc": "1320000", "#text": "\uacbd\ucc30\uccad"}, "\uc2dc\ud589\uc77c\uc790": "20160529", "\uc81c\uac1c\uc815\uad6c\ubd84": "\uc77c\ubd80\uac1c\uc815", "\ubcc4\ud45c\ud3b8\uc9d1\uc5ec\ubd80": "N", "\uacf5\ud3ec\ubc95\ub839\uc5ec\ubd80": "Y"}, "\uc870\ubb38": {"\uc870\ubb38\ub2e8\uc704": [{"@\uc870\ubb38\ud0a4": "0001001", "\uc870\ubb38\ubc88\ud638": "1", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\ubaa9\uc801", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20160529", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c1\uc870(\ubaa9\uc801) \uc774 \ubc95\uc740 \ud48d\uc18d\uc601\uc5c5(\u98a8\u4fd7\u71df\u696d)\uc744 \ud558\ub294 \uc7a5\uc18c\uc5d0\uc11c \uc120\ub7c9\ud55c \ud48d\uc18d\uc744 \ud574\uce58\uac70\ub098 \uccad\uc18c\ub144\uc758 \uac74\uc804\ud55c \uc131\uc7a5\uc744 \uc800\ud574\ud558\ub294 \ud589\uc704 \ub4f1\uc744 \uaddc\uc81c\ud558\uc5ec \ubbf8\ud48d\uc591\uc18d\uc744 \ubcf4\uc874\ud558\uace0 \uccad\uc18c\ub144\uc744 \uc720\ud574\ud55c \ud658\uacbd\uc73c\ub85c\ubd80\ud130 \ubcf4\ud638\ud568\uc744 \ubaa9\uc801\uc73c\ub85c \ud55c\ub2e4.", "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2010.7.23]"}, {"@\uc870\ubb38\ud0a4": "0002001", "\uc870\ubb38\ubc88\ud638": "2", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\ud48d\uc18d\uc601\uc5c5\uc758 \ubc94\uc704", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20160529", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c2\uc870(\ud48d\uc18d\uc601\uc5c5\uc758 \ubc94\uc704) \uc774 \ubc95\uc5d0\uc11c \"\ud48d\uc18d\uc601\uc5c5\"\uc774\ub780 \ub2e4\uc74c \uac01 \ud638\uc758 \uc5b4\ub290 \ud558\ub098\uc5d0 \ud574\ub2f9\ud558\ub294 \uc601\uc5c5\uc744 \ub9d0\ud55c\ub2e4.", "\ud56d": {"\ud638": [{"\ud638\ubc88\ud638": "1.", "\ud638\ub0b4\uc6a9": "1. \u300c\uac8c\uc784\uc0b0\uc5c5\uc9c4\ud765\uc5d0 \uad00\ud55c \ubc95\ub960\u300d \uc81c2\uc870\uc81c6\ud638\uc5d0 \ub530\ub978 \uac8c\uc784\uc81c\uacf5\uc5c5 \ubc0f \uac19\uc740 \ubc95 \uc81c2\uc870\uc81c8\ud638\uc5d0 \ub530\ub978 \ubcf5\ud569\uc720\ud1b5\uac8c\uc784\uc81c\uacf5\uc5c5"}, {"\ud638\ubc88\ud638": "2.", "\ud638\ub0b4\uc6a9": "2. \u300c\uc601\ud654 \ubc0f \ube44\ub514\uc624\ubb3c\uc758 \uc9c4\ud765\uc5d0 \uad00\ud55c \ubc95\ub960\u300d \uc81c2\uc870\uc81c16\ud638\uac00\ubaa9\uc5d0 \ub530\ub978 \ube44\ub514\uc624\ubb3c\uac10\uc0c1\uc2e4\uc5c5"}, {"\ud638\ubc88\ud638": "3.", "\ud638\ub0b4\uc6a9": "3. \u300c\uc74c\uc545\uc0b0\uc5c5\uc9c4\ud765\uc5d0 \uad00\ud55c \ubc95\ub960\u300d \uc81c2\uc870\uc81c13\ud638\uc5d0 \ub530\ub978 \ub178\ub798\uc5f0\uc2b5\uc7a5\uc5c5"}, {"\ud638\ubc88\ud638": "4.", "\ud638\ub0b4\uc6a9": "4. \u300c\uacf5\uc911\uc704\uc0dd\uad00\ub9ac\ubc95\u300d \uc81c2\uc870\uc81c1\ud56d\uc81c2\ud638\ubd80\ud130 \uc81c4\ud638\uae4c\uc9c0\uc758 \uaddc\uc815\uc5d0 \ub530\ub978 \uc219\ubc15\uc5c5, \ubaa9\uc695\uc7a5\uc5c5(\u6c90\u6d74\u5834\u696d), \uc774\uc6a9\uc5c5(\uf9e4\u5bb9\u696d) \uc911 \ub300\ud1b5\ub839\ub839\uc73c\ub85c \uc815\ud558\ub294 \uac83"}, {"\ud638\ubc88\ud638": "5.", "\ud638\ub0b4\uc6a9": "5. \u300c\uc2dd\ud488\uc704\uc0dd\ubc95\u300d \uc81c36\uc870\uc81c1\ud56d\uc81c3\ud638\uc5d0 \ub530\ub978 \uc2dd\ud488\uc811\uac1d\uc5c5 \uc911 \ub300\ud1b5\ub839\ub839\uc73c\ub85c \uc815\ud558\ub294 \uac83"}, {"\ud638\ubc88\ud638": "6.", "\ud638\ub0b4\uc6a9": "6. \u300c\uccb4\uc721\uc2dc\uc124\uc758 \uc124\uce58\u318d\uc774\uc6a9\uc5d0 \uad00\ud55c \ubc95\ub960\u300d \uc81c10\uc870\uc81c1\ud56d\uc81c2\ud638\uc5d0 \ub530\ub978 \ubb34\ub3c4\ud559\uc6d0\uc5c5 \ubc0f \ubb34\ub3c4\uc7a5\uc5c5"}, {"\ud638\ubc88\ud638": "7.", "\ud638\ub0b4\uc6a9": "7. \uadf8 \ubc16\uc5d0 \uc120\ub7c9\ud55c \ud48d\uc18d\uc744 \ud574\uce58\uac70\ub098 \uccad\uc18c\ub144\uc758 \uac74\uc804\ud55c \uc131\uc7a5\uc744 \uc800\ud574\ud560 \uc6b0\ub824\uac00 \uc788\ub294 \uc601\uc5c5\uc73c\ub85c \ub300\ud1b5\ub839\ub839\uc73c\ub85c \uc815\ud558\ub294 \uac83"}]}, "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2010.7.23]"}, {"@\uc870\ubb38\ud0a4": "0003001", "\uc870\ubb38\ubc88\ud638": "3", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uc900\uc218 \uc0ac\ud56d", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20160529", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c3\uc870(\uc900\uc218 \uc0ac\ud56d) \ud48d\uc18d\uc601\uc5c5\uc744 \ud558\ub294 \uc790(\ud5c8\uac00\ub098 \uc778\uac00\ub97c \ubc1b\uc9c0 \uc544\ub2c8\ud558\uac70\ub098 \ub4f1\ub85d\uc774\ub098 \uc2e0\uace0\ub97c \ud558\uc9c0 \uc544\ub2c8\ud558\uace0 \ud48d\uc18d\uc601\uc5c5\uc744 \ud558\ub294 \uc790\ub97c \ud3ec\ud568\ud55c\ub2e4. \uc774\ud558 \"\ud48d\uc18d\uc601\uc5c5\uc790\"\ub77c \ud55c\ub2e4) \ubc0f \ub300\ud1b5\ub839\ub839\uc73c\ub85c \uc815\ud558\ub294 \uc885\uc0ac\uc790\ub294 \ud48d\uc18d\uc601\uc5c5\uc744 \ud558\ub294 \uc7a5\uc18c(\uc774\ud558 \"\ud48d\uc18d\uc601\uc5c5\uc18c\"\ub77c \ud55c\ub2e4)\uc5d0\uc11c \ub2e4\uc74c \uac01 \ud638\uc758 \ud589\uc704\ub97c \ud558\uc5ec\uc11c\ub294 \uc544\ub2c8 \ub41c\ub2e4.", "\ud56d": {"\ud638": [{"\ud638\ubc88\ud638": "1.", "\ud638\ub0b4\uc6a9": "1. \u300c\uc131\ub9e4\ub9e4\uc54c\uc120 \ub4f1 \ud589\uc704\uc758 \ucc98\ubc8c\uc5d0 \uad00\ud55c \ubc95\ub960\u300d \uc81c2\uc870\uc81c1\ud56d\uc81c2\ud638\uc5d0 \ub530\ub978 \uc131\ub9e4\ub9e4\uc54c\uc120\ub4f1\ud589\uc704"}, {"\ud638\ubc88\ud638": "2.", "\ud638\ub0b4\uc6a9": "2. \uc74c\ub780\ud589\uc704\ub97c \ud558\uac8c \ud558\uac70\ub098 \uc774\ub97c \uc54c\uc120 \ub610\ub294 \uc81c\uacf5\ud558\ub294 \ud589\uc704"}, {"\ud638\ubc88\ud638": "3.", "\ud638\ub0b4\uc6a9": "3. \uc74c\ub780\ud55c \ubb38\uc11c\u318d\ub3c4\ud654(\u5716\u7575)\u318d\uc601\ud654\u318d\uc74c\ubc18\u318d\ube44\ub514\uc624\ubb3c, \uadf8 \ubc16\uc758 \uc74c\ub780\ud55c \ubb3c\uac74\uc5d0 \ub300\ud55c \ub2e4\uc74c \uac01 \ubaa9\uc758 \ud589\uc704", "\ubaa9": [{"\ubaa9\ubc88\ud638": "\uac00.", "\ubaa9\ub0b4\uc6a9": "\uac00. \ubc18\ud3ec(\u9812\u5e03)\u318d\ud310\ub9e4\u318d\ub300\uc5ec\ud558\uac70\ub098 \uc774\ub97c \ud558\uac8c \ud558\ub294 \ud589\uc704"}, {"\ubaa9\ubc88\ud638": "\ub098.", "\ubaa9\ub0b4\uc6a9": "\ub098. \uad00\ub78c\u318d\uc5f4\ub78c\ud558\uac8c \ud558\ub294 \ud589\uc704"}, {"\ubaa9\ubc88\ud638": "\ub2e4.", "\ubaa9\ub0b4\uc6a9": "\ub2e4. \ubc18\ud3ec\u318d\ud310\ub9e4\u318d\ub300\uc5ec\u318d\uad00\ub78c\u318d\uc5f4\ub78c\uc758 \ubaa9\uc801\uc73c\ub85c \uc9c4\uc5f4\ud558\uac70\ub098 \ubcf4\uad00\ud558\ub294 \ud589\uc704"}]}, {"\ud638\ubc88\ud638": "4.", "\ud638\ub0b4\uc6a9": "4. \ub3c4\ubc15\uc774\ub098 \uadf8 \ubc16\uc758 \uc0ac\ud589(\u5c04\u5016)\ud589\uc704\ub97c \ud558\uac8c \ud558\ub294 \ud589\uc704"}]}, "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2010.7.23]"}, {"@\uc870\ubb38\ud0a4": "0004001", "\uc870\ubb38\ubc88\ud638": "4", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\ud48d\uc18d\uc601\uc5c5\uc758 \ud1b5\ubcf4", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20160529", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c4\uc870(\ud48d\uc18d\uc601\uc5c5\uc758 \ud1b5\ubcf4)", "\ud56d": [{"\ud56d\ubc88\ud638": "\u2460", "\ud56d\ub0b4\uc6a9": "\u2460 \ub2e4\ub978 \ubc95\ub960\uc5d0 \ub530\ub77c \ud48d\uc18d\uc601\uc5c5\uc758 \ud5c8\uac00\ub97c \ud55c \uc790(\uc778\uac00\ub97c \ud558\uac70\ub098 \ub4f1\ub85d\u318d\uc2e0\uace0\ub97c \uc811\uc218\ud55c \uc790\ub97c \ud3ec\ud568\ud55c\ub2e4. \uc774\ud558 \"\ud5c8\uac00\uad00\uccad\"\uc774\ub77c \ud55c\ub2e4)\ub294 \ud48d\uc18d\uc601\uc5c5\uc18c\uc758 \uc18c\uc7ac\uc9c0\ub97c \uad00\ud560\ud558\ub294 \uacbd\ucc30\uc11c\uc7a5(\uc774\ud558 \"\uacbd\ucc30\uc11c\uc7a5\"\uc774\ub77c \ud55c\ub2e4)\uc5d0\uac8c \ub2e4\uc74c \uac01 \ud638\uc758 \uc0ac\ud56d\uc744 \uc54c\ub824\uc57c \ud55c\ub2e4.", "\ud638": [{"\ud638\ubc88\ud638": "1.", "\ud638\ub0b4\uc6a9": "1. \ud48d\uc18d\uc601\uc5c5\uc790\uc758 \uc131\uba85 \ubc0f \uc8fc\uc18c(\ubc95\uc778\uc778 \uacbd\uc6b0\uc5d0\ub294 \ub300\ud45c\uc790\uc758 \uc131\uba85\uacfc \uc8fc\uc18c\ub97c \ud3ec\ud568\ud55c\ub2e4)"}, {"\ud638\ubc88\ud638": "2.", "\ud638\ub0b4\uc6a9": "2. \ud48d\uc18d\uc601\uc5c5\uc18c\uc758 \uba85\uce6d \ubc0f \uc8fc\uc18c"}, {"\ud638\ubc88\ud638": "3.", "\ud638\ub0b4\uc6a9": "3. \ud48d\uc18d\uc601\uc5c5\uc758 \uc885\ub958"}]}, {"\ud56d\ubc88\ud638": "\u2461", "\ud56d\ub0b4\uc6a9": "\u2461 \ud5c8\uac00\uad00\uccad\uc740 \ud48d\uc18d\uc601\uc5c5\uc790\uac00 \ud734\uc5c5\u318d\ud3d0\uc5c5\ud558\uac70\ub098 \uadf8 \uc601\uc5c5\ub0b4\uc6a9\uc774 \ubcc0\uacbd\ub41c \uacbd\uc6b0\uc640 \uadf8 \ubc16\uc5d0 \ub300\ud1b5\ub839\ub839\uc73c\ub85c \uc815\ud558\ub294 \uc0ac\uc720\uac00 \ubc1c\uc0dd\ud55c \uacbd\uc6b0\uc5d0\ub294 \uacbd\ucc30\uc11c\uc7a5\uc5d0\uac8c \uadf8 \uc0ac\uc2e4\uc744 \uc54c\ub824\uc5