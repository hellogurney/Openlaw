{"root": {"\ubc95\ub839": {"@\ubc95\ub839\ud0a4": "0060722015120300373", "\uae30\ubcf8\uc815\ubcf4": {"\ubc95\ub839ID": "006072", "\uacf5\ud3ec\uc77c\uc790": "20151203", "\uacf5\ud3ec\ubc88\ud638": "00373", "\uc5b8\uc5b4": "\ud55c\uae00", "\ubc95\uc885\uad6c\ubd84": {"@\ubc95\uc885\uad6c\ubd84\ucf54\ub4dc": "A0024", "#text": "\ud5cc\ubc95\uc7ac\ud310\uc18c\uaddc\uce59"}, "\ubc95\ub839\uba85_\ud55c\uae00": "\uc2ec\ud310\uae30\ub85d \uc5f4\ub78c\u318d\ubcf5\uc0ac \ub4f1\uc5d0 \uad00\ud55c \uaddc\uce59", "\ubc95\ub839\uba85_\ud55c\uc790": "\uc2ec\ud310\uae30\ub85d \uc5f4\ub78c\u318d\ubcf5\uc0ac \ub4f1\uc5d0 \uad00\ud55c \uaddc\uce59", "\ubc95\ub839\uba85\uc57d\uce6d": null, "\uc81c\uba85\ubcc0\uacbd\uc5ec\ubd80": "N", "\ud55c\uae00\ubc95\ub839\uc5ec\ubd80": "N", "\ud3b8\uc7a5\uc808\uad00": "01030000", "\uc18c\uad00\ubd80\ucc98": {"@\uc18c\uad00\ubd80\ucc98\ucf54\ub4dc": "9750000", "#text": "\ud5cc\ubc95\uc7ac\ud310\uc18c"}, "\uc2dc\ud589\uc77c\uc790": "20160101", "\uc81c\uac1c\uc815\uad6c\ubd84": "\uc77c\ubd80\uac1c\uc815", "\ubcc4\ud45c\ud3b8\uc9d1\uc5ec\ubd80": "Y", "\uacf5\ud3ec\ubc95\ub839\uc5ec\ubd80": "Y"}, "\uc870\ubb38": {"\uc870\ubb38\ub2e8\uc704": [{"@\uc870\ubb38\ud0a4": "0001001", "\uc870\ubb38\ubc88\ud638": "1", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\ubaa9\uc801", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20160101", "\uc870\ubb38\uc81c\uac1c\uc815\uc720\ud615": "\uac1c\uc815", "\uc870\ubb38\uc81c\uac1c\uc815\uc77c\uc790\ubb38\uc790\uc5f4": "2003.12.29", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c1\uc870(\ubaa9\uc801) \uc774 \uaddc\uce59\uc740 \uc2ec\ud310\uae30\ub85d\uc758 \uc5f4\ub78c\u00b7\ubcf5\uc0ac, \uacb0\uc815\uc11c\u00b7\uc870\uc11c\uc758 \uc815\ubcf8\u00b7\ub4f1\ubcf8\u00b7\ucd08\ubcf8 \ubc0f \uc0ac\uac74\uc5d0 \uad00\ud55c \uc0ac\ud56d\uc758 \uc99d\uba85\uc11c\uc758 \uad50\ubd80\uc5d0 \ub300\ud55c \uc218\uc218\ub8cc \ub4f1\uc5d0 \uad00\ud558\uc5ec \ud544\uc694\ud55c \uc0ac\ud56d\uc744 \uaddc\uc815\ud568\uc744 \ubaa9\uc801\uc73c\ub85c \ud55c\ub2e4. <\uac1c\uc815 2003.12.29>"}, {"@\uc870\ubb38\ud0a4": "0002001", "\uc870\ubb38\ubc88\ud638": "2", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uc6a9\uc5b4\uc758 \uc815\uc758", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20160101", "\uc870\ubb38\uc81c\uac1c\uc815\uc720\ud615": "\uac1c\uc815", "\uc870\ubb38\uc81c\uac1c\uc815\uc77c\uc790\ubb38\uc790\uc5f4": "2003.12.29", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c2\uc870(\uc6a9\uc5b4\uc758 \uc815\uc758) \uc774 \uaddc\uce59\uc5d0\uc11c \uc0ac\uc6a9\ud558\ub294 \uc6a9\uc5b4\uc758 \uc815\uc758\ub294 \ub2e4\uc74c \uac01\ud638\uc640 \uac19\ub2e4. <\uac1c\uc815 2003.12.29>", "\ud56d": {"\ud638": [{"\ud638\ubc88\ud638": "1.", "\ud638\ub0b4\uc6a9": "1. \"\uc2ec\ud310\"\uc774\ub77c \ud568\uc740 \ud5cc\ubc95\uc7ac\ud310\uc18c\ubc95 \uc81c2\uc870\uc5d0 \uc758\ud55c \uac01\uc885 \uc2ec\ud310\uc0ac\uac74\uacfc \uc2e0\uccad\uc0ac\uac74 \ubc0f \ud2b9\ubcc4\uc0ac\uac74\uc5d0 \ub300\ud55c \ud5cc\ubc95\uc7ac\ud310\uc18c\uc758 \uc2ec\ud310\uc744 \ub9d0\ud55c\ub2e4."}, {"\ud638\ubc88\ud638": "2.", "\ud638\ub0b4\uc6a9": "2. \"\uc2ec\ud310\uae30\ub85d\"\uc774\ub77c \ud568\uc740 \uc2ec\ud310\uc0ac\ubb34 \ub4f1\uc5d0 \uad00\ud55c \ubb38\uc11c\uc640 \uae30\ub85d, \uc99d\uac70\ubb3c \uae30\ud0c0 \uad00\uacc4\uc11c\ub958(\ub3c4\uba74, \uc0ac\uc9c4, \ub514\uc2a4\ud06c, \ud14c\uc774\ud504, \ud544\ub984, \uc2ac\ub77c\uc774\ub4dc, \uc804\uc790\ubb38\uc11c \ub4f1\uc758 \ud2b9\uc218\ub9e4\uccb4\uae30\ub85d\uc744 \ud3ec\ud568\ud55c\ub2e4)\ub97c \ub9d0\ud55c\ub2e4."}]}}, {"@\uc870\ubb38\ud0a4": "0003001", "\uc870\ubb38\ubc88\ud638": "3", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uc801\uc6a9\ubc94\uc704", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20160101", "\uc870\ubb38\uc81c\uac1c\uc815\uc720\ud615": "\uac1c\uc815", "\uc870\ubb38\uc81c\uac1c\uc815\uc77c\uc790\ubb38\uc790\uc5f4": "2003.12.29", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c3\uc870(\uc801\uc6a9\ubc94\uc704) \uc2ec\ud310\uae30\ub85d\uc758 \uc5f4\ub78c\u00b7\ubcf5\uc0ac, \uacb0\uc815\uc11c\u00b7\uc870\uc11c\uc758 \uc815\ubcf8\u00b7\ub4f1\ubcf8\u00b7\ucd08\ubcf8 \ubc0f \uc0ac\uac74\uc5d0 \uad00\ud55c \uc0ac\ud56d\uc758 \uc99d\uba85\uc11c\uc758 \uad50\ubd80\uc5d0 \uad00\ud558\uc5ec\ub294 \ub2e4\ub978 \ubc95\ub839\uc5d0 \ud2b9\ubcc4\ud55c \uaddc\uc815\uc774 \uc788\ub294 \uacbd\uc6b0\ub97c \uc81c\uc678\ud558\uace0\ub294 \uc774 \uaddc\uce59\uc774 \uc815\ud558\ub294 \ubc14\uc5d0 \uc758\ud55c\ub2e4. <\uac1c\uc815 2003.12.29>"}, {"@\uc870\ubb38\ud0a4": "0004001", "\uc870\ubb38\ubc88\ud638": "4", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\ubcf5\uc0ac\uc758 \ubc29\uc2dd", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20160101", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c4\uc870(\ubcf5\uc0ac\uc758 \ubc29\uc2dd)", "\ud56d": [{"\ud56d\ubc88\ud638": "\u2460", "\ud56d\uc81c\uac1c\uc815\uc720\ud615": "\uac1c\uc815", "\ud56d\uc81c\uac1c\uc815\uc77c\uc790\ubb38\uc790\uc5f4": "2003.12.29", "\ud56d\ub0b4\uc6a9": "\u2460\uc2ec\ud310\uae30\ub85d \ubcf5\uc0ac\uc2e0\uccad\uc778\uc740 \ud5cc\ubc95\uc7ac\ud310\uc18c\uac00 \uc9c0\uc815\ud55c \uc7a5\uc18c\uc5d0\uc11c \uc2ec\ud310\uae30\ub85d\uc744 \uc2a4\uc2a4\ub85c \ud544\uc0ac\ud558\uac70\ub098 \ubcf5\uc0ac\uc2e0\uccad\uc778\uc758 \uc124\ube44\ub97c \uc774\uc6a9\ud558\uc5ec \ubcf5\uc0ac\ud560 \uc218 \uc788\ub2e4. <\uac1c\uc815 2003.12.29>"}, {"\ud56d\ubc88\ud638": "\u2461", "\ud56d\uc81c\uac1c\uc815\uc720\ud615": "\uac1c\uc815", "\ud56d\uc81c\uac1c\uc815\uc77c\uc790\ubb38\uc790\uc5f4": "2003.12.29", "\ud56d\ub0b4\uc6a9": "\u2461\uc2ec\ud310\uae30\ub85d \ubcf5\uc0ac\uc2e0\uccad\uc778\uc740 \ud544\uc694\ud55c \ubd80\ubd84\uc744 \ud2b9\uc815\ud558\uc5ec \ud5cc\ubc95\uc7ac\ud310\uc18c\uc758 \ubcf5\uc0ac\uae30 \ub4f1 \ud5cc\ubc95\uc7ac\ud310\uc18c\uc124\ube44\ub97c \uc774\uc6a9\ud55c \ubcf5\uc0ac\ubb3c\uc758 \uad50\ubd80\ub97c \uc2e0\uccad\ud560 \uc218 \uc788\ub2e4. <\uac1c\uc815 2003.12.29>"}], "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc81c\ubaa9\uac1c\uc815 2003.12.29]"}, {"@\uc870\ubb38\ud0a4": "0005001", "\uc870\ubb38\ubc88\ud638": "5", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\ube44\uc6a9\ubd80\ub2f4", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20160101", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": "Y", "\uc870\ubb38\ub0b4\uc6a9": "\uc81c5\uc870(\ube44\uc6a9\ubd80\ub2f4)", "\ud56d": [{"\ud56d\ubc88\ud638": "\u2460", "\ud56d\uc81c\uac1c\uc815\uc720\ud615": "\uac1c\uc815", "\ud56d\uc81c\uac1c\uc815\uc77c\uc790\ubb38\uc790\uc5f4": "2003.12.29, 2008.5.7, 2015.12.3", "\ud56d\ub0b4\uc6a9": "\u2460\uc2ec\ud310\uae30\ub85d\uc758 \uc5f4\ub78c\u00b7\ubcf5\uc0ac, \uacb0\uc815\uc11c\u00b7\uc870\uc11c\uc758 \uc815\ubcf8\u00b7\ub4f1\ubcf8\u00b7\ucd08\ubcf8 \ubc0f \uc0ac\uac74\uc5d0 \uad00\ud55c \uc0ac\ud56d\uc758 \uc99d\uba85\uc11c\uc758 \uad50\ubd80\ub97c \uc2e0\uccad\ud560 \uacbd\uc6b0 \uc2e0\uccad\uc778\uc774 \ub0a9\ubd80\ud558\uc5ec\uc57c \ud560 \uc218\uc218\ub8cc\uc758 \uae08\uc561\uc740 \ub2e4\uc74c \uac01\ud638\uc640 \uac19\uc73c\uba70, \uc6b0\ud3b8\uc73c\ub85c \uc1a1\ubd80\ubc1b\ub294 \uacbd\uc6b0\uc5d0\ub294 \uc218\uc218\ub8cc \uc678\uc5d0 \uc6b0\ud3b8\uc694\uae08\uc744 \ub0a9\ubd80\ud558\uc5ec\uc57c \ud55c\ub2e4. <\uac1c\uc815 2003.12.29, 2008.5.7, 2015.12.3>", "\ud638": [{"\ud638\ubc88\ud638": "1.", "\ud638\ub0b4\uc6a9": "1. \uae30\ub85d\uc758 \uc5f4\ub78c\u00b7\ubcf5\uc0ac\n\n    1\uac74\ub9c8\ub2e4 500\uc6d0(\uc774 \uacbd\uc6b0 \ubcf5\uc0ac\uac00 \uc5f4\ub78c\uacfc \ub3d9\uc2dc\uc5d0 \ub610\ub294 \uc5f4\ub78c\ud6c4 \uc989\uc2dc \uc774\ub8e8\uc5b4\uc9c0\ub294 \ub54c\uc5d0\ub294 1\uac74\uc758 \ubcf5\uc0ac\ub85c \ubcf8\ub2e4)"}, {"\ud638\ubc88\ud638": "2.", "\ud638\ub0b4\uc6a9": "2. \uacb0\uc815\uc11c\u00b7\uc870\uc11c\uc758 \uc815\ubcf8\u00b7\ub4f1\ubcf8\u00b7\ucd08\ubcf8\uc758 \uad50\ubd80\n\n    \uc6d0\ubcf8 5\uc7a5\uae4c\uc9c0 1000\uc6d0, \ucd08\uacfc 1\uc7a5\ub9c8\ub2e4 50\uc6d0"}, {"\ud638\ubc88\ud638": "3.", "\ud638\ub0b4\uc6a9": "3. \uc0ac\uac74\uc5d0 \uad00\ud55c \uc99d\uba85\uc11c\uc758 \uad50\ubd80\n\n    1\uac74\ub9c8\ub2e4 500\uc6d0"}]}, {"\ud56d\ubc88\ud638": "\u2461", "\ud56d\uc81c\uac1c\uc815\uc720\ud615": "\uac1c\uc815", "\ud56d\uc81c\uac1c\uc815\uc77c\uc790\ubb38\uc790\uc5f4": "2015.12.3", "\ud56d\ub0b4\uc6a9": "\u2461 \uc81c1\ud56d\uc758 \uaddc\uc815\uc5d0 \uc758\ud55c \ube44\uc6a9\uc740 \uc218\uc785\uc778\uc9c0\ub85c \ub0a9\ubd80\ud55c\ub2e4. \ub2e4\ub9cc, \uc218\uc785\uc778\uc9c0\ub85c \ub0a9\ubd80\ud558\uae30 \uc5b4\ub824\uc6b4 \uacbd\uc6b0 \uc774\ub97c \ud604\uae08\uc73c\ub85c \ub0a9\ubd80\ud560 \uc218 \uc788\ub2e4. <\uac1c\uc815 2015.12.3>"}, {"\ud56d\ubc88\ud638": "\u2462", "\ud56d\uc81c\uac1c\uc815\uc720\ud615": "\uac1c\uc815", "\ud56d\uc81c\uac1c\uc815\uc77c\uc790\ubb38\uc790\uc5f4": "2003.12.29", "\ud56d\ub0b4\uc6a9": "\u2462\uc0ac\uac74\uc758 \ub2f9\uc0ac\uc790 \ubc0f \uadf8 \ubc95\uc815\ub300\ub9ac\uc778, \uc18c\uc1a1\ub300\ub9ac\uc778\ub4f1\uc774 \uadf8 \uc0ac\uac74\uc758 \uacc4\uc18d\uc911\uc5d0 \uc2ec\ud310\uae30\ub85d\uc758 \uc5f4\ub78c\u00b7\ubcf5\uc0ac\ub97c \ud558\ub294 \ub54c\uc5d0\ub294 \uc81c1\ud56d\uc81c1\ud638\uc5d0 \uaddc\uc815\ub41c \uc218\uc218\ub8cc\ub97c \ub0a9\ubd80\ud558\uc9c0 \uc544\ub2c8\ud55c\ub2e4. \ub2e4\ub9cc, \ubcf5\uc0ac\ubb3c\uc758 \uad50\ubd80\ub97c \ubc1b\uc740 \uacbd\uc6b0\uc5d0\ub294 \ubcf5\uc0ac\ubb3c 1\uc7a5\ub9c8\ub2e4 50\uc6d0\uc758 \uc218\uc218\ub8cc\ub97c \ub0a9\ubd80\ud558\uc5ec\uc57c \ud55c\ub2e4. <\uac1c\uc815 2003.12.29>"}, {"\ud56d\ubc88\ud638": "\u2463", "\ud56d\uc81c\uac1c\uc815\uc720\ud615": "\uac1c\uc815", "\ud56d\uc81c\uac1c\uc815\uc77c\uc790\ubb38\uc790\uc5f4": "2003.12.29", "\ud56d\ub0b4\uc6a9": "\u2463\uc81c4\uc870\uc81c2\ud56d\uc758 \uacbd\uc6b0\uc5d0\ub294 \uc81c1\ud56d\uc81c1\ud638\uc5d0 \uaddc\uc815\ub41c \uc218\uc218\ub8cc\ub97c \ub0a9\ubd80\ud558\ub294 \uc678\uc5d0 \uad50\ubd80\ubc1b\uc740 \ubcf5\uc0ac\ubb3c 1\uc7a5\ub9c8\ub2e4 50\uc6d0\uc758 \uc218\uc218\ub8cc\ub97c \ub0a9\ubd80\ud558\uc5ec\uc57c \ud55c\ub2e4. <\uac1c\uc815 2003.12.29>"}, {"\ud56d\ubc88\ud638": "\u2464", "\ud56d\uc81c\uac1c\uc815\uc720\ud615": "\uac1c\uc815", "\ud56d\uc81c\uac1c\uc815\uc77c\uc790\ubb38\uc790\uc5f4": "2003.12.29, 2004.9.22, 2008.5.7", "\ud56d\ub0b4\uc6a9": "\u2464\uc2ec\ud310\uae30\ub85d\uc911 \ub179\uc74c\ud14c\uc774\ud504, \ub179\ud654\ud14c\uc774\ud504, \uc601\ud654\ud544\ub984, \uc2ac\ub77c\uc774\ub4dc, \ub9c8\uc774\ud06c\ub85c\ud544\ub984, \uc0ac\uc9c4\ud544\ub984\uc5d0 \ub300\ud55c \uc5f4\ub78c(\uccad\ucde8\u00b7\uc2dc\uccad\uc744 \ud3ec\ud568\ud55c\ub2e4)\u00b7\ubcf5\uc0ac(\ubcf5\uc81c\u00b7\uc778\ud654\ub97c \ud3ec\ud568\ud55c\ub2e4)\ub97c \uc2e0\uccad\ud558\ub294 \uc790\uac00 \ub0a9\ubd80\ud558\uc5ec\uc57c \ud560 \uc218\uc218\ub8cc\uc758 \uae08\uc561\uc5d0 \uad00\ud558\uc5ec\ub294 \u300c\ud5cc\ubc95\uc7ac\ud310\uc18c\uc815\ubcf4\uacf5\uac1c\uaddc\uce59\u300d \uc81c18\uc870\uc81c1\ud56d\uc758 \uaddc\uc815\uc744 \uc900\uc6a9\ud55c\ub2e4. \ub2e4\ub9cc, \uc0ac\uac74\uc758 \ub2f9\uc0ac\uc790 \ubc0f \uadf8 \ubc95\uc815\ub300\ub9ac\uc778, \uc18c\uc1a1\ub300\ub9ac\uc778 \ub4f1\uc774 \uadf8 \uc0ac\uac74\uc758 \uacc4\uc18d\uc911\uc5d0 \uc870\uc11c\uc758 \uc77c\ubd80\uc778 \ub179\uc74c\ud14c\uc774\ud504, \ub179\ud654\ud14c\uc774\ud504\uc5d0 \ub300\ud55c \uc5f4\ub78c\u00b7\ubcf5\uc0ac\ub97c \ud558\ub294 \uacbd\uc6b0 \uc218\uc218\ub8cc\uc758 \uae08\uc561\uc740 \ubcc4\ud45c 1\uacfc \uac19\ub2e4. <\uac1c\uc815 2003.12.29, 2004.9.22, 2008.5.7>"}, {"\ud56d\ubc88\ud638": "\u2465", "\ud56d\uc81c\uac1c\uc815\uc720\ud615": "\uc2e0\uc124", "\ud56d\uc81c\uac1c\uc815\uc77c\uc790\ubb38\uc790\uc5f4": "2008.5.7", "\ud56d\ub0b4\uc6a9": "\u2465 \uc870\uc11c\uc758 \uc77c\ubd80\uc778 \ub179\uc74c\ud14c\uc774\ud504, \ub179\ud654\ud14c\uc774\ud504\uc758 \ub4f1\ubcf8\u00b7\ucd08\ubcf8\uc758 \uad50\ubd80\ub97c \uc2e0\uccad\ud558\ub294 \uc790\uac00 \ub0a9\ubd80\ud558\uc5ec\uc57c \ud560 \uc218\uc218\ub8cc\uc758 \uae08\uc561\uc740 \ubcc4\ud45c 2\uc640 \uac19\ub2e4. <\uc2e0\uc124 2008.5.7>"}, {"\ud56d\ubc88\ud638": "\u2466", "\ud56d\uc81c\uac1c\uc815\uc720\ud615": "\uc2e0\uc124", "\ud56d\uc81c\uac1c\uc815\uc77c\uc790\ubb38\uc790\uc5f4": "2008.5.7", "\ud56d\ub0b4\uc6a9": "\u2466 \uc81c3\ud56d \ub0b4\uc9c0 \uc81c5\ud56d\uc758 \uaddc\uc815\uc5d0\ub3c4 \ubd88\uad6c\ud558\uace0 \uad6d\uc120\ub300\ub9ac\uc778\uc774 \uadf8 \uc0ac\uac74\uc758 \uacc4\uc18d\uc911\uc5d0 \uc2ec\ud310\uae30\ub85d \ubcf5\uc0ac\ubb3c\uc744 \uad50\ubd80\ubc1b\uc740 \uacbd\uc6b0\uc5d0\ub294 \ud5cc\ubc95\uc7ac\ud310\uc18c\uc7a5\uc774 \uc815\ud558\ub294 \ubc14\uc5d0 \ub530\ub77c \uc218\uc218\ub8cc\uc758 \uc77c\ubd80 \ub610\ub294 \uc804\ubd80\ub97c \uba74\uc81c\ud560 \uc218 \uc788\ub2e4. <\uc2e0\uc124 2008.5.7>"}], "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc81c\ubaa9\uac1c\uc815 2015.12.3]"}, {"@\uc870\ubb38\ud0a4": "0006001", "\uc870\ubb38\ubc88\ud638": "6", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\ubcf4\uce59", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20160101", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c6\uc870(\ubcf4\uce59)", "\ud56d": [{"\ud56d\ubc88\ud638": "\u2460", "\ud56d\uc81c\uac1c\uc815\uc720\ud615": "\uac1c\uc815", "\ud56d\uc81c\uac1c\uc815\uc77c\uc790\ubb38\uc790\uc5f4": "2003.12.29", "\ud56d\ub0b4\uc6a9": "