{"root": {"\ubc95\ub839": {"@\ubc95\ub839\ud0a4": "0001882017032114695", "\uae30\ubcf8\uc815\ubcf4": {"\ubc95\ub839ID": "000188", "\uacf5\ud3ec\uc77c\uc790": "20170321", "\uacf5\ud3ec\ubc88\ud638": "14695", "\uc5b8\uc5b4": "\ud55c\uae00", "\ubc95\uc885\uad6c\ubd84": {"@\ubc95\uc885\uad6c\ubd84\ucf54\ub4dc": "A0002", "#text": "\ubc95\ub960"}, "\ubc95\ub839\uba85_\ud55c\uae00": "\ubcf4\ud638\uc2dc\uc124\uc5d0 \uc788\ub294 \ubbf8\uc131\ub144\uc790\uc758 \ud6c4\uacac \uc9c1\ubb34\uc5d0 \uad00\ud55c \ubc95\ub960", "\ubc95\ub839\uba85_\ud55c\uc790": "\ubcf4\ud638\uc2dc\uc124\uc5d0 \uc788\ub294 \ubbf8\uc131\ub144\uc790\uc758 \ud6c4\uacac \uc9c1\ubb34\uc5d0 \uad00\ud55c \ubc95\ub960", "\ubc95\ub839\uba85\uc57d\uce6d": "\uc2dc\uc124\ubbf8\uc131\ub144\ud6c4\uacac\ubc95", "\uc81c\uba85\ubcc0\uacbd\uc5ec\ubd80": "N", "\ud55c\uae00\ubc95\ub839\uc5ec\ubd80": "N", "\ud3b8\uc7a5\uc808\uad00": "38050000", "\uc18c\uad00\ubd80\ucc98": {"@\uc18c\uad00\ubd80\ucc98\ucf54\ub4dc": "1352000", "#text": "\ubcf4\uac74\ubcf5\uc9c0\ubd80"}, "\uc2dc\ud589\uc77c\uc790": "20170622", "\uc81c\uac1c\uc815\uad6c\ubd84": "\uc77c\ubd80\uac1c\uc815", "\ubcc4\ud45c\ud3b8\uc9d1\uc5ec\ubd80": "Y", "\uacf5\ud3ec\ubc95\ub839\uc5ec\ubd80": "Y"}, "\uc870\ubb38": {"\uc870\ubb38\ub2e8\uc704": [{"@\uc870\ubb38\ud0a4": "0001001", "\uc870\ubb38\ubc88\ud638": "1", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\ubaa9\uc801", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20170622", "\uc870\ubb38\uc81c\uac1c\uc815\uc720\ud615": "\uc804\ubb38\uac1c\uc815", "\uc870\ubb38\uc81c\uac1c\uc815\uc77c\uc790\ubb38\uc790\uc5f4": "2012.10.22", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c1\uc870(\ubaa9\uc801) \uc774 \ubc95\uc740 \ubcf4\ud638\uc2dc\uc124\uc5d0 \uc788\ub294 \ubbf8\uc131\ub144\uc790\uc758 \ud6c4\uacac\uc778(\u5f8c\u898b\u4eba)\uc5d0 \uad00\ud55c \uc0ac\ud56d\uc744 \uaddc\uc815\ud568\uc744 \ubaa9\uc801\uc73c\ub85c \ud55c\ub2e4.", "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2012.10.22]"}, {"@\uc870\ubb38\ud0a4": "0002001", "\uc870\ubb38\ubc88\ud638": "2", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\uc815\uc758", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20170622", "\uc870\ubb38\uc81c\uac1c\uc815\uc720\ud615": "\uc804\ubb38\uac1c\uc815", "\uc870\ubb38\uc81c\uac1c\uc815\uc77c\uc790\ubb38\uc790\uc5f4": "2012.10.22", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c2\uc870(\uc815\uc758) \uc774 \ubc95\uc5d0\uc11c \uc0ac\uc6a9\ud558\ub294 \uc6a9\uc5b4\uc758 \ub73b\uc740 \ub2e4\uc74c\uacfc \uac19\ub2e4. <\uac1c\uc815 2016.2.3, 2016.5.29, 2017.3.21>", "\ud56d": {"\ud638": [{"\ud638\ubc88\ud638": "1.", "\ud638\ub0b4\uc6a9": "1. \"\ubcf4\ud638\uc2dc\uc124\"\uc774\ub780 \ub2e4\uc74c \uac01 \ubaa9\uc758 \uc2dc\uc124\uc744 \ub9d0\ud55c\ub2e4.", "\ubaa9": [{"\ubaa9\ubc88\ud638": "\uac00.", "\ubaa9\ub0b4\uc6a9": "\uac00. \u300c\uc544\ub3d9\ubcf5\uc9c0\ubc95\u300d \uc81c52\uc870\uc5d0 \ub530\ub978 \uc544\ub3d9\ubcf5\uc9c0\uc2dc\uc124 \uc911 \ub300\ud1b5\ub839\ub839\uc73c\ub85c \uc815\ud558\ub294 \uc2dc\uc124"}, {"\ubaa9\ubc88\ud638": "\ub098.", "\ubaa9\ub0b4\uc6a9": "\ub098. \u300c\uc7a5\uc560\uc778\ubcf5\uc9c0\ubc95\u300d \uc81c58\uc870\uc81c1\ud56d\uc81c1\ud638\uc758 \uc7a5\uc560\uc778 \uac70\uc8fc\uc2dc\uc124"}, {"\ubaa9\ubc88\ud638": "\ub2e4.", "\ubaa9\ub0b4\uc6a9": "\ub2e4. \u300c\uc815\uc2e0\uac74\uac15\uc99d\uc9c4 \ubc0f \uc815\uc2e0\uc9c8\ud658\uc790 \ubcf5\uc9c0\uc11c\ube44\uc2a4 \uc9c0\uc6d0\uc5d0 \uad00\ud55c \ubc95\ub960\u300d \uc81c22\uc870\uc5d0 \ub530\ub978 \uc815\uc2e0\uc694\uc591\uc2dc\uc124 \ubc0f \uac19\uc740 \ubc95 \uc81c26\uc870\uc5d0 \ub530\ub978 \uc815\uc2e0\uc7ac\ud65c\uc2dc\uc124"}, {"\ubaa9\ubc88\ud638": "\ub77c.", "\ubaa9\ub0b4\uc6a9": "\ub77c. \u300c\ud55c\ubd80\ubaa8\uac00\uc871\uc9c0\uc6d0\ubc95\u300d \uc81c19\uc870\uc81c1\ud56d\uc81c3\ud638\uc758 \ubbf8\ud63c\ubaa8\uc790\uac00\uc871\ubcf5\uc9c0\uc2dc\uc124"}, {"\ubaa9\ubc88\ud638": "\ub9c8.", "\ubaa9\ub0b4\uc6a9": "\ub9c8. \u300c\ub178\uc219\uc778 \ub4f1\uc758 \ubcf5\uc9c0 \ubc0f \uc790\ub9bd\uc9c0\uc6d0\uc5d0 \uad00\ud55c \ubc95\ub960\u300d \uc81c16\uc870\uc81c1\ud56d\uc81c2\ud638\ubd80\ud130 \uc81c4\ud638\uae4c\uc9c0\uc758 \ub178\uc219\uc778\uc790\ud65c\uc2dc\uc124, \ub178\uc219\uc778\uc7ac\ud65c\uc2dc\uc124 \ubc0f \ub178\uc219\uc778\uc694\uc591\uc2dc\uc124"}, {"\ubaa9\ubc88\ud638": "\ubc14.", "\ubaa9\ub0b4\uc6a9": "\ubc14. \u300c\uccad\uc18c\ub144\ubcf5\uc9c0 \uc9c0\uc6d0\ubc95\u300d \uc81c31\uc870\uc81c1\ud638 \ubc0f \uc81c2\ud638\uc758 \uccad\uc18c\ub144\uc27c\ud130 \ubc0f \uccad\uc18c\ub144\uc790\ub9bd\uc9c0\uc6d0\uad00"}, {"\ubaa9\ubc88\ud638": "\uc0ac.", "\ubaa9\ub0b4\uc6a9": "\uc0ac. \u300c\uc131\ud3ed\ub825\ubc29\uc9c0 \ubc0f \ud53c\ud574\uc790\ubcf4\ud638 \ub4f1\uc5d0 \uad00\ud55c \ubc95\ub960\u300d \uc81c12\uc870\uc81c3\ud56d\uc81c1\ud638\ubd80\ud130 \uc81c3\ud638\uae4c\uc9c0\uc758 \uc77c\ubc18\ubcf4\ud638\uc2dc\uc124, \uc7a5\uc560\uc778\ubcf4\ud638\uc2dc\uc124, \ud2b9\ubcc4\uc9c0\uc6d0 \ubcf4\ud638\uc2dc\uc124"}, {"\ubaa9\ubc88\ud638": "\uc544.", "\ubaa9\ub0b4\uc6a9": "\uc544. \u300c\uc131\ub9e4\ub9e4\ubc29\uc9c0 \ubc0f \ud53c\ud574\uc790\ubcf4\ud638 \ub4f1\uc5d0 \uad00\ud55c \ubc95\ub960\u300d \uc81c9\uc870\uc81c1\ud56d\uc81c1\ud638\u00b7\uc81c2\ud638 \ubc0f \uc81c4\ud638\uc758 \uc77c\ubc18 \uc9c0\uc6d0\uc2dc\uc124\uacfc \uccad\uc18c\ub144 \uc9c0\uc6d0\uc2dc\uc124, \uc790\ub9bd\uc9c0\uc6d0 \uacf5\ub3d9\uc0dd\ud65c\uc2dc\uc124"}]}, {"\ud638\ubc88\ud638": "2.", "\ud638\ub0b4\uc6a9": "2. \"\ubd80\uc591\uc758\ubb34\uc790\"\ub780 \ubcf4\ud638\uc2dc\uc124\uc5d0 \uc788\ub294 \ubbf8\uc131\ub144\uc790\uc5d0 \ub300\ud558\uc5ec \u300c\ubbfc\ubc95\u300d \uc81c931\uc870 \ubc0f \uc81c932\uc870\uc5d0 \ub530\ub77c \ud6c4\uacac\uc778\uc774 \ub420 \uc218 \uc788\ub294 \uc0ac\ub78c\uc744 \ub9d0\ud55c\ub2e4."}]}, "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2012.10.22]"}, {"@\uc870\ubb38\ud0a4": "0003001", "\uc870\ubb38\ubc88\ud638": "3", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\ud6c4\uacac\uc778", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20170622", "\uc870\ubb38\uc81c\uac1c\uc815\uc720\ud615": "\uc804\ubb38\uac1c\uc815", "\uc870\ubb38\uc81c\uac1c\uc815\uc77c\uc790\ubb38\uc790\uc5f4": "2012.10.22", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c3\uc870(\ud6c4\uacac\uc778)", "\ud56d": [{"\ud56d\ubc88\ud638": "\u2460", "\ud56d\ub0b4\uc6a9": "\u2460 \uad6d\uac00\ub098 \uc9c0\ubc29\uc790\uce58\ub2e8\uccb4\uac00 \uc124\uce58\u00b7\uc6b4\uc601\ud558\ub294 \ubcf4\ud638\uc2dc\uc124\uc5d0 \uc788\ub294 \ubbf8\uc131\ub144\uc790\uc778 \uace0\uc544\uc5d0 \ub300\ud558\uc5ec\ub294 \uadf8 \ubcf4\ud638\uc2dc\uc124\uc758 \uc7a5\uc774 \ud6c4\uacac\uc778\uc774 \ub41c\ub2e4."}, {"\ud56d\ubc88\ud638": "\u2461", "\ud56d\ub0b4\uc6a9": "\u2461 \uad6d\uac00 \ub610\ub294 \uc9c0\ubc29\uc790\uce58\ub2e8\uccb4 \uc678\uc758 \uc790\uac00 \uc124\uce58\u00b7\uc6b4\uc601\ud558\ub294 \ubcf4\ud638\uc2dc\uc124\uc5d0 \uc788\ub294 \ubbf8\uc131\ub144\uc790\uc778 \uace0\uc544\uc5d0 \ub300\ud558\uc5ec\ub294 \ub300\ud1b5\ub839\ub839\uc73c\ub85c \uc815\ud558\ub294 \ubc14\uc5d0 \ub530\ub77c \uadf8 \ubcf4\ud638\uc2dc\uc124\uc758 \uc18c\uc7ac\uc9c0\ub97c \uad00\ud560\ud558\ub294 \ud2b9\ubcc4\uc790\uce58\uc2dc\uc7a5\u00b7\uc2dc\uc7a5(\u300c\uc81c\uc8fc\ud2b9\ubcc4\uc790\uce58\ub3c4 \uc124\uce58 \ubc0f \uad6d\uc81c\uc790\uc720\ub3c4\uc2dc \uc870\uc131\uc744 \uc704\ud55c \ud2b9\ubcc4\ubc95\u300d\uc5d0 \ub530\ub978 \ud589\uc815\uc2dc\uc7a5\uc744 \ud3ec\ud568\ud55c\ub2e4. \uc774\ud558 \uac19\ub2e4)\u00b7\uad70\uc218\u00b7\uad6c\uccad\uc7a5(\uc790\uce58\uad6c\uc758 \uad6c\uccad\uc7a5\uc744 \ub9d0\ud55c\ub2e4. \uc774\ud558 \uac19\ub2e4)\uc774 \ud6c4\uacac\uc778\uc744 \uc9c0\uc815\ud55c\ub2e4."}, {"\ud56d\ubc88\ud638": "\u2462", "\ud56d\ub0b4\uc6a9": "\u2462 \ubcf4\ud638\uc2dc\uc124\uc5d0 \uc788\ub294 \uace0\uc544\uac00 \uc544\ub2cc \ubbf8\uc131\ub144\uc790\uc5d0 \ub300\ud558\uc5ec\ub3c4 \uc81c1\ud56d\uacfc \uc81c2\ud56d\uc744 \uc900\uc6a9\ud558\ub418, \ub300\ud1b5\ub839\ub839\uc73c\ub85c \uc815\ud558\ub294 \ubc14\uc5d0 \ub530\ub77c \ubc95\uc6d0\uc758 \ud5c8\uac00\ub97c \ubc1b\uc544\uc57c \ud55c\ub2e4."}], "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2012.10.22]"}, {"@\uc870\ubb38\ud0a4": "0004001", "\uc870\ubb38\ubc88\ud638": "4", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\ubd80\uc591\uc758\ubb34\uc790 \ud655\uc778 \uacf5\uace0", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20170622", "\uc870\ubb38\uc81c\uac1c\uc815\uc720\ud615": "\uc804\ubb38\uac1c\uc815", "\uc870\ubb38\uc81c\uac1c\uc815\uc77c\uc790\ubb38\uc790\uc5f4": "2012.10.22", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c4\uc870(\ubd80\uc591\uc758\ubb34\uc790 \ud655\uc778 \uacf5\uace0)", "\ud56d": [{"\ud56d\ubc88\ud638": "\u2460", "\ud56d\ub0b4\uc6a9": "\u2460 \ubcf4\ud638\uc2dc\uc124\uc758 \uc7a5\uc740 \uc81c3\uc870\uc5d0 \ub530\ub978 \ud6c4\uacac\uc778 \uc9c0\uc815 \ub4f1\uc5d0 \uc788\uc5b4 \ub300\ud1b5\ub839\ub839\uc73c\ub85c \uc815\ud558\ub294 \ubc14\uc5d0 \ub530\ub77c \ud574\ub2f9 \ubcf4\ud638\uc2dc\uc124\uc758 \uc18c\uc7ac\uc9c0\ub97c \uad00\ud560\ud558\ub294 \ud2b9\ubcc4\uc790\uce58\uc2dc\uc7a5\u00b7\uc2dc\uc7a5\u00b7\uad70\uc218\u00b7\uad6c\uccad\uc7a5\uc5d0\uac8c \ubd80\uc591\uc758\ubb34\uc790 \ud655\uc778 \uacf5\uace0\ub97c \uc758\ub8b0\ud558\uc5ec\uc57c \ud55c\ub2e4. \ub2e4\ub9cc, \ub2e4\ub978 \ubcf4\ud638\uc2dc\uc124\uc758 \uc7a5\uc774 \ud655\uc778 \uacf5\uace0\ub97c \uc758\ub8b0\ud55c \uc0ac\uc2e4\uc774 \uc788\ub294 \uc0ac\ub78c\uc5d0 \ub300\ud558\uc5ec\ub294 \uadf8\ub7ec\ud558\uc9c0 \uc544\ub2c8\ud558\ub2e4."}, {"\ud56d\ubc88\ud638": "\u2461", "\ud56d\ub0b4\uc6a9": "\u2461 \uc81c1\ud56d\uc5d0 \ub530\ub77c \ubd80\uc591\uc758\ubb34\uc790 \ud655\uc778 \uacf5\uace0\ub97c \uc758\ub8b0\ubc1b\uc740 \ud2b9\ubcc4\uc790\uce58\uc2dc\uc7a5\u00b7\uc2dc\uc7a5\u00b7\uad70\uc218\u00b7\uad6c\uccad\uc7a5\uc740 \ub300\ud1b5\ub839\ub839\uc73c\ub85c \uc815\ud558\ub294 \uae30\uac04 \ub3d9\uc548 \uacf5\uace0\ub97c \ud558\uc5ec\uc57c \ud55c\ub2e4."}], "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\uc815 2012.10.22]"}, {"@\uc870\ubb38\ud0a4": "0005001", "\uc870\ubb38\ubc88\ud638": "5", "\uc870\ubb38\uc5ec\ubd80": "\uc870\ubb38", "\uc870\ubb38\uc81c\ubaa9": "\ud6c4\uacac\uc778\uc758 \uacb0\uaca9\uc0ac\uc720", "\uc870\ubb38\uc2dc\ud589\uc77c\uc790": "20170622", "\uc870\ubb38\uc81c\uac1c\uc815\uc720\ud615": "\uc804\ubb38\uac1c\uc815", "\uc870\ubb38\uc81c\uac1c\uc815\uc77c\uc790\ubb38\uc790\uc5f4": "2012.10.22", "\uc870\ubb38\uc774\ub3d9\uc774\uc804": "000000", "\uc870\ubb38\uc774\ub3d9\uc774\ud6c4": "000000", "\uc870\ubb38\ubcc0\uacbd\uc5ec\ubd80": null, "\uc870\ubb38\ub0b4\uc6a9": "\uc81c5\uc870(\ud6c4\uacac\uc778\uc758 \uacb0\uaca9\uc0ac\uc720) \ub2e4\uc74c \uac01 \ud638\uc758 \uc5b4\ub290 \ud558\ub098\uc5d0 \ud574\ub2f9\ud558\ub294 \uc0ac\ub78c\uc740 \ud6c4\uacac\uc778\uc774 \ub420 \uc218 \uc5c6\ub2e4.", "\ud56d": {"\ud638": [{"\ud638\ubc88\ud638": "1.", "\ud638\ub0b4\uc6a9": "1. \u300c\ubbfc\ubc95\u300d \uc81c937\uc870 \uac01 \ud638\uc758 \uc5b4\ub290 \ud558\ub098\uc5d0 \ud574\ub2f9\ud558\ub294 \uc0ac\ub78c"}, {"\ud638\ubc88\ud638": "2.", "\ud638\ub0b4\uc6a9": "2. \ud6c4\uacac\uc778\uc758 \uc9c1\ubb34\ub97c \uc131\uc2e4\ud788 \uc218\ud589\ud560 \uc218 \uc5c6\uac70\ub098 \uc544\ub3d9\ubcf5\uc9c0\ub97c \uc704\ud558\uc5ec \ubd80\uc801\ub2f9\ud558\ub2e4\uace0 \uc778\uc815\ub418\ub294 \uc0ac\ub78c"}]}, "\uc870\ubb38\ucc38\uace0\uc790\ub8cc": "[\uc804\ubb38\uac1c\